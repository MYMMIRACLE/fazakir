[
{
    "id": "5759",
    "sura": "80",
    "VerseIDAr": "1",
    "ayat": "عَبَسَ وَتَوَلَّىٰ"
},
{
    "id": "5760",
    "sura": "80",
    "VerseIDAr": "2",
    "ayat": "أَنْ جَاءَهُ الْأَعْمَىٰ"
},
{
    "id": "5761",
    "sura": "80",
    "VerseIDAr": "3",
    "ayat": "وَمَا يُدْرِيكَ لَعَلَّهُ يَزَّكَّىٰ"
},
{
    "id": "5762",
    "sura": "80",
    "VerseIDAr": "4",
    "ayat": "أَوْ يَذَّكَّرُ فَتَنْفَعَهُ الذِّكْرَىٰ"
},
{
    "id": "5763",
    "sura": "80",
    "VerseIDAr": "5",
    "ayat": "أَمَّا مَنِ اسْتَغْنَىٰ"
},
{
    "id": "5764",
    "sura": "80",
    "VerseIDAr": "6",
    "ayat": "فَأَنْتَ لَهُ تَصَدَّىٰ"
},
{
    "id": "5765",
    "sura": "80",
    "VerseIDAr": "7",
    "ayat": "وَمَا عَلَيْكَ أَلَّا يَزَّكَّىٰ"
},
{
    "id": "5766",
    "sura": "80",
    "VerseIDAr": "8",
    "ayat": "وَأَمَّا مَنْ جَاءَكَ يَسْعَىٰ"
},
{
    "id": "5767",
    "sura": "80",
    "VerseIDAr": "9",
    "ayat": "وَهُوَ يَخْشَىٰ"
},
{
    "id": "5768",
    "sura": "80",
    "VerseIDAr": "10",
    "ayat": "فَأَنْتَ عَنْهُ تَلَهَّىٰ"
},
{
    "id": "5769",
    "sura": "80",
    "VerseIDAr": "11",
    "ayat": "كَلَّا إِنَّهَا تَذْكِرَةٌ"
},
{
    "id": "5770",
    "sura": "80",
    "VerseIDAr": "12",
    "ayat": "فَمَنْ شَاءَ ذَكَرَهُ"
},
{
    "id": "5771",
    "sura": "80",
    "VerseIDAr": "13",
    "ayat": "فِي صُحُفٍ مُكَرَّمَةٍ"
},
{
    "id": "5772",
    "sura": "80",
    "VerseIDAr": "14",
    "ayat": "مَرْفُوعَةٍ مُطَهَّرَةٍ"
},
{
    "id": "5773",
    "sura": "80",
    "VerseIDAr": "15",
    "ayat": "بِأَيْدِي سَفَرَةٍ"
},
{
    "id": "5774",
    "sura": "80",
    "VerseIDAr": "16",
    "ayat": "كِرَامٍ بَرَرَةٍ"
},
{
    "id": "5775",
    "sura": "80",
    "VerseIDAr": "17",
    "ayat": "قُتِلَ الْإِنْسَانُ مَا أَكْفَرَهُ"
},
{
    "id": "5776",
    "sura": "80",
    "VerseIDAr": "18",
    "ayat": "مِنْ أَيِّ شَيْءٍ خَلَقَهُ"
},
{
    "id": "5777",
    "sura": "80",
    "VerseIDAr": "19",
    "ayat": "مِنْ نُطْفَةٍ خَلَقَهُ فَقَدَّرَهُ"
},
{
    "id": "5778",
    "sura": "80",
    "VerseIDAr": "20",
    "ayat": "ثُمَّ السَّبِيلَ يَسَّرَهُ"
},
{
    "id": "5779",
    "sura": "80",
    "VerseIDAr": "21",
    "ayat": "ثُمَّ أَمَاتَهُ فَأَقْبَرَهُ"
},
{
    "id": "5780",
    "sura": "80",
    "VerseIDAr": "22",
    "ayat": "ثُمَّ إِذَا شَاءَ أَنْشَرَهُ"
},
{
    "id": "5781",
    "sura": "80",
    "VerseIDAr": "23",
    "ayat": "كَلَّا لَمَّا يَقْضِ مَا أَمَرَهُ"
},
{
    "id": "5782",
    "sura": "80",
    "VerseIDAr": "24",
    "ayat": "فَلْيَنْظُرِ الْإِنْسَانُ إِلَىٰ طَعَامِهِ"
},
{
    "id": "5783",
    "sura": "80",
    "VerseIDAr": "25",
    "ayat": "أَنَّا صَبَبْنَا الْمَاءَ صَبًّا"
},
{
    "id": "5784",
    "sura": "80",
    "VerseIDAr": "26",
    "ayat": "ثُمَّ شَقَقْنَا الْأَرْضَ شَقًّا"
},
{
    "id": "5785",
    "sura": "80",
    "VerseIDAr": "27",
    "ayat": "فَأَنْبَتْنَا فِيهَا حَبًّا"
},
{
    "id": "5786",
    "sura": "80",
    "VerseIDAr": "28",
    "ayat": "وَعِنَبًا وَقَضْبًا"
},
{
    "id": "5787",
    "sura": "80",
    "VerseIDAr": "29",
    "ayat": "وَزَيْتُونًا وَنَخْلًا"
},
{
    "id": "5788",
    "sura": "80",
    "VerseIDAr": "30",
    "ayat": "وَحَدَائِقَ غُلْبًا"
},
{
    "id": "5789",
    "sura": "80",
    "VerseIDAr": "31",
    "ayat": "وَفَاكِهَةً وَأَبًّا"
},
{
    "id": "5790",
    "sura": "80",
    "VerseIDAr": "32",
    "ayat": "مَتَاعًا لَكُمْ وَلِأَنْعَامِكُمْ"
},
{
    "id": "5791",
    "sura": "80",
    "VerseIDAr": "33",
    "ayat": "فَإِذَا جَاءَتِ الصَّاخَّةُ"
},
{
    "id": "5792",
    "sura": "80",
    "VerseIDAr": "34",
    "ayat": "يَوْمَ يَفِرُّ الْمَرْءُ مِنْ أَخِيهِ"
},
{
    "id": "5793",
    "sura": "80",
    "VerseIDAr": "35",
    "ayat": "وَأُمِّهِ وَأَبِيهِ"
},
{
    "id": "5794",
    "sura": "80",
    "VerseIDAr": "36",
    "ayat": "وَصَاحِبَتِهِ وَبَنِيهِ"
},
{
    "id": "5795",
    "sura": "80",
    "VerseIDAr": "37",
    "ayat": "لِكُلِّ امْرِئٍ مِنْهُمْ يَوْمَئِذٍ شَأْنٌ يُغْنِيهِ"
},
{
    "id": "5796",
    "sura": "80",
    "VerseIDAr": "38",
    "ayat": "وُجُوهٌ يَوْمَئِذٍ مُسْفِرَةٌ"
},
{
    "id": "5797",
    "sura": "80",
    "VerseIDAr": "39",
    "ayat": "ضَاحِكَةٌ مُسْتَبْشِرَةٌ"
},
{
    "id": "5798",
    "sura": "80",
    "VerseIDAr": "40",
    "ayat": "وَوُجُوهٌ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌ"
},
{
    "id": "5799",
    "sura": "80",
    "VerseIDAr": "41",
    "ayat": "تَرْهَقُهَا قَتَرَةٌ"
},
{
    "id": "5800",
    "sura": "80",
    "VerseIDAr": "42",
    "ayat": "أُولَٰئِكَ هُمُ الْكَفَرَةُ الْفَجَرَةُ"
}
]