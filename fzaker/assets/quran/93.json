[
{
    "id": "6080",
    "sura": "93",
    "VerseIDAr": "1",
    "ayat": " وَالضُّحَىٰ"
},
{
    "id": "6081",
    "sura": "93",
    "VerseIDAr": "2",
    "ayat": "وَاللَّيْلِ إِذَا سَجَىٰ"
},
{
    "id": "6082",
    "sura": "93",
    "VerseIDAr": "3",
    "ayat": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ"
},
{
    "id": "6083",
    "sura": "93",
    "VerseIDAr": "4",
    "ayat": "وَلَلْآخِرَةُ خَيْرٌ لَكَ مِنَ الْأُولَىٰ"
},
{
    "id": "6084",
    "sura": "93",
    "VerseIDAr": "5",
    "ayat": "وَلَسَوْفَ يُعْطِيكَ رَبُّكَ فَتَرْضَىٰ"
},
{
    "id": "6085",
    "sura": "93",
    "VerseIDAr": "6",
    "ayat": "أَلَمْ يَجِدْكَ يَتِيمًا فَآوَىٰ"
},
{
    "id": "6086",
    "sura": "93",
    "VerseIDAr": "7",
    "ayat": "وَوَجَدَكَ ضَالًّا فَهَدَىٰ"
},
{
    "id": "6087",
    "sura": "93",
    "VerseIDAr": "8",
    "ayat": "وَوَجَدَكَ عَائِلًا فَأَغْنَىٰ"
},
{
    "id": "6088",
    "sura": "93",
    "VerseIDAr": "9",
    "ayat": "فَأَمَّا الْيَتِيمَ فَلَا تَقْهَرْ"
},
{
    "id": "6089",
    "sura": "93",
    "VerseIDAr": "10",
    "ayat": "وَأَمَّا السَّائِلَ فَلَا تَنْهَرْ"
},
{
    "id": "6090",
    "sura": "93",
    "VerseIDAr": "11",
    "ayat": "وَأَمَّا بِنِعْمَةِ رَبِّكَ فَحَدِّثْ"
}
]