[
{
    "id": "5910",
    "sura": "85",
    "VerseIDAr": "1",
    "ayat": "وَالسَّمَاءِ ذَاتِ الْبُرُوجِ"
},
{
    "id": "5911",
    "sura": "85",
    "VerseIDAr": "2",
    "ayat": "وَالْيَوْمِ الْمَوْعُودِ"
},
{
    "id": "5912",
    "sura": "85",
    "VerseIDAr": "3",
    "ayat": "وَشَاهِدٍ وَمَشْهُودٍ"
},
{
    "id": "5913",
    "sura": "85",
    "VerseIDAr": "4",
    "ayat": "قُتِلَ أَصْحَابُ الْأُخْدُودِ"
},
{
    "id": "5914",
    "sura": "85",
    "VerseIDAr": "5",
    "ayat": "النَّارِ ذَاتِ الْوَقُودِ"
},
{
    "id": "5915",
    "sura": "85",
    "VerseIDAr": "6",
    "ayat": "إِذْ هُمْ عَلَيْهَا قُعُودٌ"
},
{
    "id": "5916",
    "sura": "85",
    "VerseIDAr": "7",
    "ayat": "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِالْمُؤْمِنِينَ شُهُودٌ"
},
{
    "id": "5917",
    "sura": "85",
    "VerseIDAr": "8",
    "ayat": "وَمَا نَقَمُوا مِنْهُمْ إِلَّا أَنْ يُؤْمِنُوا بِاللَّهِ الْعَزِيزِ الْحَمِيدِ"
},
{
    "id": "5918",
    "sura": "85",
    "VerseIDAr": "9",
    "ayat": "الَّذِي لَهُ مُلْكُ السَّمَاوَاتِ وَالْأَرْضِ ۚ وَاللَّهُ عَلَىٰ كُلِّ شَيْءٍ شَهِيدٌ"
},
{
    "id": "5919",
    "sura": "85",
    "VerseIDAr": "10",
    "ayat": "إِنَّ الَّذِينَ فَتَنُوا الْمُؤْمِنِينَ وَالْمُؤْمِنَاتِ ثُمَّ لَمْ يَتُوبُوا فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ الْحَرِيقِ"
},
{
    "id": "5920",
    "sura": "85",
    "VerseIDAr": "11",
    "ayat": "إِنَّ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَهُمْ جَنَّاتٌ تَجْرِي مِنْ تَحْتِهَا الْأَنْهَارُ ۚ ذَٰلِكَ الْفَوْزُ الْكَبِيرُ"
},
{
    "id": "5921",
    "sura": "85",
    "VerseIDAr": "12",
    "ayat": "إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌ"
},
{
    "id": "5922",
    "sura": "85",
    "VerseIDAr": "13",
    "ayat": "إِنَّهُ هُوَ يُبْدِئُ وَيُعِيدُ"
},
{
    "id": "5923",
    "sura": "85",
    "VerseIDAr": "14",
    "ayat": "وَهُوَ الْغَفُورُ الْوَدُودُ"
},
{
    "id": "5924",
    "sura": "85",
    "VerseIDAr": "15",
    "ayat": "ذُو الْعَرْشِ الْمَجِيدُ"
},
{
    "id": "5925",
    "sura": "85",
    "VerseIDAr": "16",
    "ayat": "فَعَّالٌ لِمَا يُرِيدُ"
},
{
    "id": "5926",
    "sura": "85",
    "VerseIDAr": "17",
    "ayat": "هَلْ أَتَاكَ حَدِيثُ الْجُنُودِ"
},
{
    "id": "5927",
    "sura": "85",
    "VerseIDAr": "18",
    "ayat": "فِرْعَوْنَ وَثَمُودَ"
},
{
    "id": "5928",
    "sura": "85",
    "VerseIDAr": "19",
    "ayat": "بَلِ الَّذِينَ كَفَرُوا فِي تَكْذِيبٍ"
},
{
    "id": "5929",
    "sura": "85",
    "VerseIDAr": "20",
    "ayat": "وَاللَّهُ مِنْ وَرَائِهِمْ مُحِيطٌ"
},
{
    "id": "5930",
    "sura": "85",
    "VerseIDAr": "21",
    "ayat": "بَلْ هُوَ قُرْآنٌ مَجِيدٌ"
},
{
    "id": "5931",
    "sura": "85",
    "VerseIDAr": "22",
    "ayat": "فِي لَوْحٍ مَحْفُوظٍ"
}
]