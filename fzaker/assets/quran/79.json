[
{
    "id": "5713",
    "sura": "79",
    "VerseIDAr": "1",
    "ayat": "وَالنَّازِعَاتِ غَرْقًا"
},
{
    "id": "5714",
    "sura": "79",
    "VerseIDAr": "2",
    "ayat": "وَالنَّاشِطَاتِ نَشْطًا"
},
{
    "id": "5715",
    "sura": "79",
    "VerseIDAr": "3",
    "ayat": "وَالسَّابِحَاتِ سَبْحًا"
},
{
    "id": "5716",
    "sura": "79",
    "VerseIDAr": "4",
    "ayat": "فَالسَّابِقَاتِ سَبْقًا"
},
{
    "id": "5717",
    "sura": "79",
    "VerseIDAr": "5",
    "ayat": "فَالْمُدَبِّرَاتِ أَمْرًا"
},
{
    "id": "5718",
    "sura": "79",
    "VerseIDAr": "6",
    "ayat": "يَوْمَ تَرْجُفُ الرَّاجِفَةُ"
},
{
    "id": "5719",
    "sura": "79",
    "VerseIDAr": "7",
    "ayat": "تَتْبَعُهَا الرَّادِفَةُ"
},
{
    "id": "5720",
    "sura": "79",
    "VerseIDAr": "8",
    "ayat": "قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ"
},
{
    "id": "5721",
    "sura": "79",
    "VerseIDAr": "9",
    "ayat": "أَبْصَارُهَا خَاشِعَةٌ"
},
{
    "id": "5722",
    "sura": "79",
    "VerseIDAr": "10",
    "ayat": "يَقُولُونَ أَإِنَّا لَمَرْدُودُونَ فِي الْحَافِرَةِ"
},
{
    "id": "5723",
    "sura": "79",
    "VerseIDAr": "11",
    "ayat": "أَإِذَا كُنَّا عِظَامًا نَخِرَةً"
},
{
    "id": "5724",
    "sura": "79",
    "VerseIDAr": "12",
    "ayat": "قَالُوا تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ"
},
{
    "id": "5725",
    "sura": "79",
    "VerseIDAr": "13",
    "ayat": "فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ"
},
{
    "id": "5726",
    "sura": "79",
    "VerseIDAr": "14",
    "ayat": "فَإِذَا هُمْ بِالسَّاهِرَةِ"
},
{
    "id": "5727",
    "sura": "79",
    "VerseIDAr": "15",
    "ayat": "هَلْ أَتَاكَ حَدِيثُ مُوسَىٰ"
},
{
    "id": "5728",
    "sura": "79",
    "VerseIDAr": "16",
    "ayat": "إِذْ نَادَاهُ رَبُّهُ بِالْوَادِ الْمُقَدَّسِ طُوًى"
},
{
    "id": "5729",
    "sura": "79",
    "VerseIDAr": "17",
    "ayat": "اذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُ طَغَىٰ"
},
{
    "id": "5730",
    "sura": "79",
    "VerseIDAr": "18",
    "ayat": "فَقُلْ هَلْ لَكَ إِلَىٰ أَنْ تَزَكَّىٰ"
},
{
    "id": "5731",
    "sura": "79",
    "VerseIDAr": "19",
    "ayat": "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ"
},
{
    "id": "5732",
    "sura": "79",
    "VerseIDAr": "20",
    "ayat": "فَأَرَاهُ الْآيَةَ الْكُبْرَىٰ"
},
{
    "id": "5733",
    "sura": "79",
    "VerseIDAr": "21",
    "ayat": "فَكَذَّبَ وَعَصَىٰ"
},
{
    "id": "5734",
    "sura": "79",
    "VerseIDAr": "22",
    "ayat": "ثُمَّ أَدْبَرَ يَسْعَىٰ"
},
{
    "id": "5735",
    "sura": "79",
    "VerseIDAr": "23",
    "ayat": "فَحَشَرَ فَنَادَىٰ"
},
{
    "id": "5736",
    "sura": "79",
    "VerseIDAr": "24",
    "ayat": "فَقَالَ أَنَا رَبُّكُمُ الْأَعْلَىٰ"
},
{
    "id": "5737",
    "sura": "79",
    "VerseIDAr": "25",
    "ayat": "فَأَخَذَهُ اللَّهُ نَكَالَ الْآخِرَةِ وَالْأُولَىٰ"
},
{
    "id": "5738",
    "sura": "79",
    "VerseIDAr": "26",
    "ayat": "إِنَّ فِي ذَٰلِكَ لَعِبْرَةً لِمَنْ يَخْشَىٰ"
},
{
    "id": "5739",
    "sura": "79",
    "VerseIDAr": "27",
    "ayat": "أَأَنْتُمْ أَشَدُّ خَلْقًا أَمِ السَّمَاءُ ۚ بَنَاهَا"
},
{
    "id": "5740",
    "sura": "79",
    "VerseIDAr": "28",
    "ayat": "رَفَعَ سَمْكَهَا فَسَوَّاهَا"
},
{
    "id": "5741",
    "sura": "79",
    "VerseIDAr": "29",
    "ayat": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَاهَا"
},
{
    "id": "5742",
    "sura": "79",
    "VerseIDAr": "30",
    "ayat": "وَالْأَرْضَ بَعْدَ ذَٰلِكَ دَحَاهَا"
},
{
    "id": "5743",
    "sura": "79",
    "VerseIDAr": "31",
    "ayat": "أَخْرَجَ مِنْهَا مَاءَهَا وَمَرْعَاهَا"
},
{
    "id": "5744",
    "sura": "79",
    "VerseIDAr": "32",
    "ayat": "وَالْجِبَالَ أَرْسَاهَا"
},
{
    "id": "5745",
    "sura": "79",
    "VerseIDAr": "33",
    "ayat": "مَتَاعًا لَكُمْ وَلِأَنْعَامِكُمْ"
},
{
    "id": "5746",
    "sura": "79",
    "VerseIDAr": "34",
    "ayat": "فَإِذَا جَاءَتِ الطَّامَّةُ الْكُبْرَىٰ"
},
{
    "id": "5747",
    "sura": "79",
    "VerseIDAr": "35",
    "ayat": "يَوْمَ يَتَذَكَّرُ الْإِنْسَانُ مَا سَعَىٰ"
},
{
    "id": "5748",
    "sura": "79",
    "VerseIDAr": "36",
    "ayat": "وَبُرِّزَتِ الْجَحِيمُ لِمَنْ يَرَىٰ"
},
{
    "id": "5749",
    "sura": "79",
    "VerseIDAr": "37",
    "ayat": "فَأَمَّا مَنْ طَغَىٰ"
},
{
    "id": "5750",
    "sura": "79",
    "VerseIDAr": "38",
    "ayat": "وَآثَرَ الْحَيَاةَ الدُّنْيَا"
},
{
    "id": "5751",
    "sura": "79",
    "VerseIDAr": "39",
    "ayat": "فَإِنَّ الْجَحِيمَ هِيَ الْمَأْوَىٰ"
},
{
    "id": "5752",
    "sura": "79",
    "VerseIDAr": "40",
    "ayat": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِ وَنَهَى النَّفْسَ عَنِ الْهَوَىٰ"
},
{
    "id": "5753",
    "sura": "79",
    "VerseIDAr": "41",
    "ayat": "فَإِنَّ الْجَنَّةَ هِيَ الْمَأْوَىٰ"
},
{
    "id": "5754",
    "sura": "79",
    "VerseIDAr": "42",
    "ayat": "يَسْأَلُونَكَ عَنِ السَّاعَةِ أَيَّانَ مُرْسَاهَا"
},
{
    "id": "5755",
    "sura": "79",
    "VerseIDAr": "43",
    "ayat": "فِيمَ أَنْتَ مِنْ ذِكْرَاهَا"
},
{
    "id": "5756",
    "sura": "79",
    "VerseIDAr": "44",
    "ayat": "إِلَىٰ رَبِّكَ مُنْتَهَاهَا"
},
{
    "id": "5757",
    "sura": "79",
    "VerseIDAr": "45",
    "ayat": "إِنَّمَا أَنْتَ مُنْذِرُ مَنْ يَخْشَاهَا"
},
{
    "id": "5758",
    "sura": "79",
    "VerseIDAr": "46",
    "ayat": "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوا إِلَّا عَشِيَّةً أَوْ ضُحَاهَا"
}
]