[
{
    "id": "6158",
    "sura": "101",
    "VerseIDAr": "1",
    "ayat": "الْقَارِعَةُ"
},
{
    "id": "6159",
    "sura": "101",
    "VerseIDAr": "2",
    "ayat": "مَا الْقَارِعَةُ"
},
{
    "id": "6160",
    "sura": "101",
    "VerseIDAr": "3",
    "ayat": "وَمَا أَدْرَاكَ مَا الْقَارِعَةُ"
},
{
    "id": "6161",
    "sura": "101",
    "VerseIDAr": "4",
    "ayat": "يَوْمَ يَكُونُ النَّاسُ كَالْفَرَاشِ الْمَبْثُوثِ"
},
{
    "id": "6162",
    "sura": "101",
    "VerseIDAr": "5",
    "ayat": "وَتَكُونُ الْجِبَالُ كَالْعِهْنِ الْمَنْفُوشِ"
},
{
    "id": "6163",
    "sura": "101",
    "VerseIDAr": "6",
    "ayat": "فَأَمَّا مَنْ ثَقُلَتْ مَوَازِينُهُ"
},
{
    "id": "6164",
    "sura": "101",
    "VerseIDAr": "7",
    "ayat": "فَهُوَ فِي عِيشَةٍ رَاضِيَةٍ"
},
{
    "id": "6165",
    "sura": "101",
    "VerseIDAr": "8",
    "ayat": "وَأَمَّا مَنْ خَفَّتْ مَوَازِينُهُ"
},
{
    "id": "6166",
    "sura": "101",
    "VerseIDAr": "9",
    "ayat": "فَأُمُّهُ هَاوِيَةٌ"
},
{
    "id": "6167",
    "sura": "101",
    "VerseIDAr": "10",
    "ayat": "وَمَا أَدْرَاكَ مَا هِيَهْ"
},
{
    "id": "6168",
    "sura": "101",
    "VerseIDAr": "11",
    "ayat": "نَارٌ حَامِيَةٌ"
}
]