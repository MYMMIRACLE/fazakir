[
{
    "id": "6059",
    "sura": "92",
    "VerseIDAr": "1",
    "ayat": " وَاللَّيْلِ إِذَا يَغْشَىٰ"
},
{
    "id": "6060",
    "sura": "92",
    "VerseIDAr": "2",
    "ayat": "وَالنَّهَارِ إِذَا تَجَلَّىٰ"
},
{
    "id": "6061",
    "sura": "92",
    "VerseIDAr": "3",
    "ayat": "وَمَا خَلَقَ الذَّكَرَ وَالْأُنْثَىٰ"
},
{
    "id": "6062",
    "sura": "92",
    "VerseIDAr": "4",
    "ayat": "إِنَّ سَعْيَكُمْ لَشَتَّىٰ"
},
{
    "id": "6063",
    "sura": "92",
    "VerseIDAr": "5",
    "ayat": "فَأَمَّا مَنْ أَعْطَىٰ وَاتَّقَىٰ"
},
{
    "id": "6064",
    "sura": "92",
    "VerseIDAr": "6",
    "ayat": "وَصَدَّقَ بِالْحُسْنَىٰ"
},
{
    "id": "6065",
    "sura": "92",
    "VerseIDAr": "7",
    "ayat": "فَسَنُيَسِّرُهُ لِلْيُسْرَىٰ"
},
{
    "id": "6066",
    "sura": "92",
    "VerseIDAr": "8",
    "ayat": "وَأَمَّا مَنْ بَخِلَ وَاسْتَغْنَىٰ"
},
{
    "id": "6067",
    "sura": "92",
    "VerseIDAr": "9",
    "ayat": "وَكَذَّبَ بِالْحُسْنَىٰ"
},
{
    "id": "6068",
    "sura": "92",
    "VerseIDAr": "10",
    "ayat": "فَسَنُيَسِّرُهُ لِلْعُسْرَىٰ"
},
{
    "id": "6069",
    "sura": "92",
    "VerseIDAr": "11",
    "ayat": "وَمَا يُغْنِي عَنْهُ مَالُهُ إِذَا تَرَدَّىٰ"
},
{
    "id": "6070",
    "sura": "92",
    "VerseIDAr": "12",
    "ayat": "إِنَّ عَلَيْنَا لَلْهُدَىٰ"
},
{
    "id": "6071",
    "sura": "92",
    "VerseIDAr": "13",
    "ayat": "وَإِنَّ لَنَا لَلْآخِرَةَ وَالْأُولَىٰ"
},
{
    "id": "6072",
    "sura": "92",
    "VerseIDAr": "14",
    "ayat": "فَأَنْذَرْتُكُمْ نَارًا تَلَظَّىٰ"
},
{
    "id": "6073",
    "sura": "92",
    "VerseIDAr": "15",
    "ayat": "لَا يَصْلَاهَا إِلَّا الْأَشْقَى"
},
{
    "id": "6074",
    "sura": "92",
    "VerseIDAr": "16",
    "ayat": "الَّذِي كَذَّبَ وَتَوَلَّىٰ"
},
{
    "id": "6075",
    "sura": "92",
    "VerseIDAr": "17",
    "ayat": "وَسَيُجَنَّبُهَا الْأَتْقَى"
},
{
    "id": "6076",
    "sura": "92",
    "VerseIDAr": "18",
    "ayat": "الَّذِي يُؤْتِي مَالَهُ يَتَزَكَّىٰ"
},
{
    "id": "6077",
    "sura": "92",
    "VerseIDAr": "19",
    "ayat": "وَمَا لِأَحَدٍ عِنْدَهُ مِنْ نِعْمَةٍ تُجْزَىٰ"
},
{
    "id": "6078",
    "sura": "92",
    "VerseIDAr": "20",
    "ayat": "إِلَّا ابْتِغَاءَ وَجْهِ رَبِّهِ الْأَعْلَىٰ"
},
{
    "id": "6079",
    "sura": "92",
    "VerseIDAr": "21",
    "ayat": "وَلَسَوْفَ يَرْضَىٰ"
}
]