[
{
    "id": "5272",
    "sura": "68",
    "VerseIDAr": "1",
    "ayat": " ن ۚ وَالْقَلَمِ وَمَا يَسْطُرُونَ"
},
{
    "id": "5273",
    "sura": "68",
    "VerseIDAr": "2",
    "ayat": "مَا أَنْتَ بِنِعْمَةِ رَبِّكَ بِمَجْنُونٍ"
},
{
    "id": "5274",
    "sura": "68",
    "VerseIDAr": "3",
    "ayat": "وَإِنَّ لَكَ لَأَجْرًا غَيْرَ مَمْنُونٍ"
},
{
    "id": "5275",
    "sura": "68",
    "VerseIDAr": "4",
    "ayat": "وَإِنَّكَ لَعَلَىٰ خُلُقٍ عَظِيمٍ"
},
{
    "id": "5276",
    "sura": "68",
    "VerseIDAr": "5",
    "ayat": "فَسَتُبْصِرُ وَيُبْصِرُونَ"
},
{
    "id": "5277",
    "sura": "68",
    "VerseIDAr": "6",
    "ayat": "بِأَيْيِكُمُ الْمَفْتُونُ"
},
{
    "id": "5278",
    "sura": "68",
    "VerseIDAr": "7",
    "ayat": "إِنَّ رَبَّكَ هُوَ أَعْلَمُ بِمَنْ ضَلَّ عَنْ سَبِيلِهِ وَهُوَ أَعْلَمُ بِالْمُهْتَدِينَ"
},
{
    "id": "5279",
    "sura": "68",
    "VerseIDAr": "8",
    "ayat": "فَلَا تُطِعِ الْمُكَذِّبِينَ"
},
{
    "id": "5280",
    "sura": "68",
    "VerseIDAr": "9",
    "ayat": "وَدُّوا لَوْ تُدْهِنُ فَيُدْهِنُونَ"
},
{
    "id": "5281",
    "sura": "68",
    "VerseIDAr": "10",
    "ayat": "وَلَا تُطِعْ كُلَّ حَلَّافٍ مَهِينٍ"
},
{
    "id": "5282",
    "sura": "68",
    "VerseIDAr": "11",
    "ayat": "هَمَّازٍ مَشَّاءٍ بِنَمِيمٍ"
},
{
    "id": "5283",
    "sura": "68",
    "VerseIDAr": "12",
    "ayat": "مَنَّاعٍ لِلْخَيْرِ مُعْتَدٍ أَثِيمٍ"
},
{
    "id": "5284",
    "sura": "68",
    "VerseIDAr": "13",
    "ayat": "عُتُلٍّ بَعْدَ ذَٰلِكَ زَنِيمٍ"
},
{
    "id": "5285",
    "sura": "68",
    "VerseIDAr": "14",
    "ayat": "أَنْ كَانَ ذَا مَالٍ وَبَنِينَ"
},
{
    "id": "5286",
    "sura": "68",
    "VerseIDAr": "15",
    "ayat": "إِذَا تُتْلَىٰ عَلَيْهِ آيَاتُنَا قَالَ أَسَاطِيرُ الْأَوَّلِينَ"
},
{
    "id": "5287",
    "sura": "68",
    "VerseIDAr": "16",
    "ayat": "سَنَسِمُهُ عَلَى الْخُرْطُومِ"
},
{
    "id": "5288",
    "sura": "68",
    "VerseIDAr": "17",
    "ayat": "إِنَّا بَلَوْنَاهُمْ كَمَا بَلَوْنَا أَصْحَابَ الْجَنَّةِ إِذْ أَقْسَمُوا لَيَصْرِمُنَّهَا مُصْبِحِينَ"
},
{
    "id": "5289",
    "sura": "68",
    "VerseIDAr": "18",
    "ayat": "وَلَا يَسْتَثْنُونَ"
},
{
    "id": "5290",
    "sura": "68",
    "VerseIDAr": "19",
    "ayat": "فَطَافَ عَلَيْهَا طَائِفٌ مِنْ رَبِّكَ وَهُمْ نَائِمُونَ"
},
{
    "id": "5291",
    "sura": "68",
    "VerseIDAr": "20",
    "ayat": "فَأَصْبَحَتْ كَالصَّرِيمِ"
},
{
    "id": "5292",
    "sura": "68",
    "VerseIDAr": "21",
    "ayat": "فَتَنَادَوْا مُصْبِحِينَ"
},
{
    "id": "5293",
    "sura": "68",
    "VerseIDAr": "22",
    "ayat": "أَنِ اغْدُوا عَلَىٰ حَرْثِكُمْ إِنْ كُنْتُمْ صَارِمِينَ"
},
{
    "id": "5294",
    "sura": "68",
    "VerseIDAr": "23",
    "ayat": "فَانْطَلَقُوا وَهُمْ يَتَخَافَتُونَ"
},
{
    "id": "5295",
    "sura": "68",
    "VerseIDAr": "24",
    "ayat": "أَنْ لَا يَدْخُلَنَّهَا الْيَوْمَ عَلَيْكُمْ مِسْكِينٌ"
},
{
    "id": "5296",
    "sura": "68",
    "VerseIDAr": "25",
    "ayat": "وَغَدَوْا عَلَىٰ حَرْدٍ قَادِرِينَ"
},
{
    "id": "5297",
    "sura": "68",
    "VerseIDAr": "26",
    "ayat": "فَلَمَّا رَأَوْهَا قَالُوا إِنَّا لَضَالُّونَ"
},
{
    "id": "5298",
    "sura": "68",
    "VerseIDAr": "27",
    "ayat": "بَلْ نَحْنُ مَحْرُومُونَ"
},
{
    "id": "5299",
    "sura": "68",
    "VerseIDAr": "28",
    "ayat": "قَالَ أَوْسَطُهُمْ أَلَمْ أَقُلْ لَكُمْ لَوْلَا تُسَبِّحُونَ"
},
{
    "id": "5300",
    "sura": "68",
    "VerseIDAr": "29",
    "ayat": "قَالُوا سُبْحَانَ رَبِّنَا إِنَّا كُنَّا ظَالِمِينَ"
},
{
    "id": "5301",
    "sura": "68",
    "VerseIDAr": "30",
    "ayat": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَلَاوَمُونَ"
},
{
    "id": "5302",
    "sura": "68",
    "VerseIDAr": "31",
    "ayat": "قَالُوا يَا وَيْلَنَا إِنَّا كُنَّا طَاغِينَ"
},
{
    "id": "5303",
    "sura": "68",
    "VerseIDAr": "32",
    "ayat": "عَسَىٰ رَبُّنَا أَنْ يُبْدِلَنَا خَيْرًا مِنْهَا إِنَّا إِلَىٰ رَبِّنَا رَاغِبُونَ"
},
{
    "id": "5304",
    "sura": "68",
    "VerseIDAr": "33",
    "ayat": "كَذَٰلِكَ الْعَذَابُ ۖ وَلَعَذَابُ الْآخِرَةِ أَكْبَرُ ۚ لَوْ كَانُوا يَعْلَمُونَ"
},
{
    "id": "5305",
    "sura": "68",
    "VerseIDAr": "34",
    "ayat": "إِنَّ لِلْمُتَّقِينَ عِنْدَ رَبِّهِمْ جَنَّاتِ النَّعِيمِ"
},
{
    "id": "5306",
    "sura": "68",
    "VerseIDAr": "35",
    "ayat": "أَفَنَجْعَلُ الْمُسْلِمِينَ كَالْمُجْرِمِينَ"
},
{
    "id": "5307",
    "sura": "68",
    "VerseIDAr": "36",
    "ayat": "مَا لَكُمْ كَيْفَ تَحْكُمُونَ"
},
{
    "id": "5308",
    "sura": "68",
    "VerseIDAr": "37",
    "ayat": "أَمْ لَكُمْ كِتَابٌ فِيهِ تَدْرُسُونَ"
},
{
    "id": "5309",
    "sura": "68",
    "VerseIDAr": "38",
    "ayat": "إِنَّ لَكُمْ فِيهِ لَمَا تَخَيَّرُونَ"
},
{
    "id": "5310",
    "sura": "68",
    "VerseIDAr": "39",
    "ayat": "أَمْ لَكُمْ أَيْمَانٌ عَلَيْنَا بَالِغَةٌ إِلَىٰ يَوْمِ الْقِيَامَةِ ۙ إِنَّ لَكُمْ لَمَا تَحْكُمُونَ"
},
{
    "id": "5311",
    "sura": "68",
    "VerseIDAr": "40",
    "ayat": "سَلْهُمْ أَيُّهُمْ بِذَٰلِكَ زَعِيمٌ"
},
{
    "id": "5312",
    "sura": "68",
    "VerseIDAr": "41",
    "ayat": "أَمْ لَهُمْ شُرَكَاءُ فَلْيَأْتُوا بِشُرَكَائِهِمْ إِنْ كَانُوا صَادِقِينَ"
},
{
    "id": "5313",
    "sura": "68",
    "VerseIDAr": "42",
    "ayat": "يَوْمَ يُكْشَفُ عَنْ سَاقٍ وَيُدْعَوْنَ إِلَى السُّجُودِ فَلَا يَسْتَطِيعُونَ"
},
{
    "id": "5314",
    "sura": "68",
    "VerseIDAr": "43",
    "ayat": "خَاشِعَةً أَبْصَارُهُمْ تَرْهَقُهُمْ ذِلَّةٌ ۖ وَقَدْ كَانُوا يُدْعَوْنَ إِلَى السُّجُودِ وَهُمْ سَالِمُونَ"
},
{
    "id": "5315",
    "sura": "68",
    "VerseIDAr": "44",
    "ayat": "فَذَرْنِي وَمَنْ يُكَذِّبُ بِهَٰذَا الْحَدِيثِ ۖ سَنَسْتَدْرِجُهُمْ مِنْ حَيْثُ لَا يَعْلَمُونَ"
},
{
    "id": "5316",
    "sura": "68",
    "VerseIDAr": "45",
    "ayat": "وَأُمْلِي لَهُمْ ۚ إِنَّ كَيْدِي مَتِينٌ"
},
{
    "id": "5317",
    "sura": "68",
    "VerseIDAr": "46",
    "ayat": "أَمْ تَسْأَلُهُمْ أَجْرًا فَهُمْ مِنْ مَغْرَمٍ مُثْقَلُونَ"
},
{
    "id": "5318",
    "sura": "68",
    "VerseIDAr": "47",
    "ayat": "أَمْ عِنْدَهُمُ الْغَيْبُ فَهُمْ يَكْتُبُونَ"
},
{
    "id": "5319",
    "sura": "68",
    "VerseIDAr": "48",
    "ayat": "فَاصْبِرْ لِحُكْمِ رَبِّكَ وَلَا تَكُنْ كَصَاحِبِ الْحُوتِ إِذْ نَادَىٰ وَهُوَ مَكْظُومٌ"
},
{
    "id": "5320",
    "sura": "68",
    "VerseIDAr": "49",
    "ayat": "لَوْلَا أَنْ تَدَارَكَهُ نِعْمَةٌ مِنْ رَبِّهِ لَنُبِذَ بِالْعَرَاءِ وَهُوَ مَذْمُومٌ"
},
{
    "id": "5321",
    "sura": "68",
    "VerseIDAr": "50",
    "ayat": "فَاجْتَبَاهُ رَبُّهُ فَجَعَلَهُ مِنَ الصَّالِحِينَ"
},
{
    "id": "5322",
    "sura": "68",
    "VerseIDAr": "51",
    "ayat": "وَإِنْ يَكَادُ الَّذِينَ كَفَرُوا لَيُزْلِقُونَكَ بِأَبْصَارِهِمْ لَمَّا سَمِعُوا الذِّكْرَ وَيَقُولُونَ إِنَّهُ لَمَجْنُونٌ"
},
{
    "id": "5323",
    "sura": "68",
    "VerseIDAr": "52",
    "ayat": "وَمَا هُوَ إِلَّا ذِكْرٌ لِلْعَالَمِينَ"
}
]