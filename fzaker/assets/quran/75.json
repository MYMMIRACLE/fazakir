[
{
    "id": "5552",
    "sura": "75",
    "VerseIDAr": "1",
    "ayat": " لَا أُقْسِمُ بِيَوْمِ الْقِيَامَةِ"
},
{
    "id": "5553",
    "sura": "75",
    "VerseIDAr": "2",
    "ayat": "وَلَا أُقْسِمُ بِالنَّفْسِ اللَّوَّامَةِ"
},
{
    "id": "5554",
    "sura": "75",
    "VerseIDAr": "3",
    "ayat": "أَيَحْسَبُ الْإِنْسَانُ أَلَّنْ نَجْمَعَ عِظَامَهُ"
},
{
    "id": "5555",
    "sura": "75",
    "VerseIDAr": "4",
    "ayat": "بَلَىٰ قَادِرِينَ عَلَىٰ أَنْ نُسَوِّيَ بَنَانَهُ"
},
{
    "id": "5556",
    "sura": "75",
    "VerseIDAr": "5",
    "ayat": "بَلْ يُرِيدُ الْإِنْسَانُ لِيَفْجُرَ أَمَامَهُ"
},
{
    "id": "5557",
    "sura": "75",
    "VerseIDAr": "6",
    "ayat": "يَسْأَلُ أَيَّانَ يَوْمُ الْقِيَامَةِ"
},
{
    "id": "5558",
    "sura": "75",
    "VerseIDAr": "7",
    "ayat": "فَإِذَا بَرِقَ الْبَصَرُ"
},
{
    "id": "5559",
    "sura": "75",
    "VerseIDAr": "8",
    "ayat": "وَخَسَفَ الْقَمَرُ"
},
{
    "id": "5560",
    "sura": "75",
    "VerseIDAr": "9",
    "ayat": "وَجُمِعَ الشَّمْسُ وَالْقَمَرُ"
},
{
    "id": "5561",
    "sura": "75",
    "VerseIDAr": "10",
    "ayat": "يَقُولُ الْإِنْسَانُ يَوْمَئِذٍ أَيْنَ الْمَفَرُّ"
},
{
    "id": "5562",
    "sura": "75",
    "VerseIDAr": "11",
    "ayat": "كَلَّا لَا وَزَرَ"
},
{
    "id": "5563",
    "sura": "75",
    "VerseIDAr": "12",
    "ayat": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ الْمُسْتَقَرُّ"
},
{
    "id": "5564",
    "sura": "75",
    "VerseIDAr": "13",
    "ayat": "يُنَبَّأُ الْإِنْسَانُ يَوْمَئِذٍ بِمَا قَدَّمَ وَأَخَّرَ"
},
{
    "id": "5565",
    "sura": "75",
    "VerseIDAr": "14",
    "ayat": "بَلِ الْإِنْسَانُ عَلَىٰ نَفْسِهِ بَصِيرَةٌ"
},
{
    "id": "5566",
    "sura": "75",
    "VerseIDAr": "15",
    "ayat": "وَلَوْ أَلْقَىٰ مَعَاذِيرَهُ"
},
{
    "id": "5567",
    "sura": "75",
    "VerseIDAr": "16",
    "ayat": "لَا تُحَرِّكْ بِهِ لِسَانَكَ لِتَعْجَلَ بِهِ"
},
{
    "id": "5568",
    "sura": "75",
    "VerseIDAr": "17",
    "ayat": "إِنَّ عَلَيْنَا جَمْعَهُ وَقُرْآنَهُ"
},
{
    "id": "5569",
    "sura": "75",
    "VerseIDAr": "18",
    "ayat": "فَإِذَا قَرَأْنَاهُ فَاتَّبِعْ قُرْآنَهُ"
},
{
    "id": "5570",
    "sura": "75",
    "VerseIDAr": "19",
    "ayat": "ثُمَّ إِنَّ عَلَيْنَا بَيَانَهُ"
},
{
    "id": "5571",
    "sura": "75",
    "VerseIDAr": "20",
    "ayat": "كَلَّا بَلْ تُحِبُّونَ الْعَاجِلَةَ"
},
{
    "id": "5572",
    "sura": "75",
    "VerseIDAr": "21",
    "ayat": "وَتَذَرُونَ الْآخِرَةَ"
},
{
    "id": "5573",
    "sura": "75",
    "VerseIDAr": "22",
    "ayat": "وُجُوهٌ يَوْمَئِذٍ نَاضِرَةٌ"
},
{
    "id": "5574",
    "sura": "75",
    "VerseIDAr": "23",
    "ayat": "إِلَىٰ رَبِّهَا نَاظِرَةٌ"
},
{
    "id": "5575",
    "sura": "75",
    "VerseIDAr": "24",
    "ayat": "وَوُجُوهٌ يَوْمَئِذٍ بَاسِرَةٌ"
},
{
    "id": "5576",
    "sura": "75",
    "VerseIDAr": "25",
    "ayat": "تَظُنُّ أَنْ يُفْعَلَ بِهَا فَاقِرَةٌ"
},
{
    "id": "5577",
    "sura": "75",
    "VerseIDAr": "26",
    "ayat": "كَلَّا إِذَا بَلَغَتِ التَّرَاقِيَ"
},
{
    "id": "5578",
    "sura": "75",
    "VerseIDAr": "27",
    "ayat": "وَقِيلَ مَنْ ۜ رَاقٍ"
},
{
    "id": "5579",
    "sura": "75",
    "VerseIDAr": "28",
    "ayat": "وَظَنَّ أَنَّهُ الْفِرَاقُ"
},
{
    "id": "5580",
    "sura": "75",
    "VerseIDAr": "29",
    "ayat": "وَالْتَفَّتِ السَّاقُ بِالسَّاقِ"
},
{
    "id": "5581",
    "sura": "75",
    "VerseIDAr": "30",
    "ayat": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ الْمَسَاقُ"
},
{
    "id": "5582",
    "sura": "75",
    "VerseIDAr": "31",
    "ayat": "فَلَا صَدَّقَ وَلَا صَلَّىٰ"
},
{
    "id": "5583",
    "sura": "75",
    "VerseIDAr": "32",
    "ayat": "وَلَٰكِنْ كَذَّبَ وَتَوَلَّىٰ"
},
{
    "id": "5584",
    "sura": "75",
    "VerseIDAr": "33",
    "ayat": "ثُمَّ ذَهَبَ إِلَىٰ أَهْلِهِ يَتَمَطَّىٰ"
},
{
    "id": "5585",
    "sura": "75",
    "VerseIDAr": "34",
    "ayat": "أَوْلَىٰ لَكَ فَأَوْلَىٰ"
},
{
    "id": "5586",
    "sura": "75",
    "VerseIDAr": "35",
    "ayat": "ثُمَّ أَوْلَىٰ لَكَ فَأَوْلَىٰ"
},
{
    "id": "5587",
    "sura": "75",
    "VerseIDAr": "36",
    "ayat": "أَيَحْسَبُ الْإِنْسَانُ أَنْ يُتْرَكَ سُدًى"
},
{
    "id": "5588",
    "sura": "75",
    "VerseIDAr": "37",
    "ayat": "أَلَمْ يَكُ نُطْفَةً مِنْ مَنِيٍّ يُمْنَىٰ"
},
{
    "id": "5589",
    "sura": "75",
    "VerseIDAr": "38",
    "ayat": "ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوَّىٰ"
},
{
    "id": "5590",
    "sura": "75",
    "VerseIDAr": "39",
    "ayat": "فَجَعَلَ مِنْهُ الزَّوْجَيْنِ الذَّكَرَ وَالْأُنْثَىٰ"
},
{
    "id": "5591",
    "sura": "75",
    "VerseIDAr": "40",
    "ayat": "أَلَيْسَ ذَٰلِكَ بِقَادِرٍ عَلَىٰ أَنْ يُحْيِيَ الْمَوْتَىٰ"
}
]