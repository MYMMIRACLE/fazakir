[
{
    "id": "6139",
    "sura": "99",
    "VerseIDAr": "1",
    "ayat": " إِذَا زُلْزِلَتِ الْأَرْضُ زِلْزَالَهَا"
},
{
    "id": "6140",
    "sura": "99",
    "VerseIDAr": "2",
    "ayat": "وَأَخْرَجَتِ الْأَرْضُ أَثْقَالَهَا"
},
{
    "id": "6141",
    "sura": "99",
    "VerseIDAr": "3",
    "ayat": "وَقَالَ الْإِنْسَانُ مَا لَهَا"
},
{
    "id": "6142",
    "sura": "99",
    "VerseIDAr": "4",
    "ayat": "يَوْمَئِذٍ تُحَدِّثُ أَخْبَارَهَا"
},
{
    "id": "6143",
    "sura": "99",
    "VerseIDAr": "5",
    "ayat": "بِأَنَّ رَبَّكَ أَوْحَىٰ لَهَا"
},
{
    "id": "6144",
    "sura": "99",
    "VerseIDAr": "6",
    "ayat": "يَوْمَئِذٍ يَصْدُرُ النَّاسُ أَشْتَاتًا لِيُرَوْا أَعْمَالَهُمْ"
},
{
    "id": "6145",
    "sura": "99",
    "VerseIDAr": "7",
    "ayat": "فَمَنْ يَعْمَلْ مِثْقَالَ ذَرَّةٍ خَيْرًا يَرَهُ"
},
{
    "id": "6146",
    "sura": "99",
    "VerseIDAr": "8",
    "ayat": "وَمَنْ يَعْمَلْ مِثْقَالَ ذَرَّةٍ شَرًّا يَرَهُ"
}
]