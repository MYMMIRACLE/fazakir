[
{
    "id": "5673",
    "sura": "78",
    "VerseIDAr": "1",
    "ayat": "عَمَّ يَتَسَاءَلُونَ"
},
{
    "id": "5674",
    "sura": "78",
    "VerseIDAr": "2",
    "ayat": "عَنِ النَّبَإِ الْعَظِيمِ"
},
{
    "id": "5675",
    "sura": "78",
    "VerseIDAr": "3",
    "ayat": "الَّذِي هُمْ فِيهِ مُخْتَلِفُونَ"
},
{
    "id": "5676",
    "sura": "78",
    "VerseIDAr": "4",
    "ayat": "كَلَّا سَيَعْلَمُونَ"
},
{
    "id": "5677",
    "sura": "78",
    "VerseIDAr": "5",
    "ayat": "ثُمَّ كَلَّا سَيَعْلَمُونَ"
},
{
    "id": "5678",
    "sura": "78",
    "VerseIDAr": "6",
    "ayat": "أَلَمْ نَجْعَلِ الْأَرْضَ مِهَادًا"
},
{
    "id": "5679",
    "sura": "78",
    "VerseIDAr": "7",
    "ayat": "وَالْجِبَالَ أَوْتَادًا"
},
{
    "id": "5680",
    "sura": "78",
    "VerseIDAr": "8",
    "ayat": "وَخَلَقْنَاكُمْ أَزْوَاجًا"
},
{
    "id": "5681",
    "sura": "78",
    "VerseIDAr": "9",
    "ayat": "وَجَعَلْنَا نَوْمَكُمْ سُبَاتًا"
},
{
    "id": "5682",
    "sura": "78",
    "VerseIDAr": "10",
    "ayat": "وَجَعَلْنَا اللَّيْلَ لِبَاسًا"
},
{
    "id": "5683",
    "sura": "78",
    "VerseIDAr": "11",
    "ayat": "وَجَعَلْنَا النَّهَارَ مَعَاشًا"
},
{
    "id": "5684",
    "sura": "78",
    "VerseIDAr": "12",
    "ayat": "وَبَنَيْنَا فَوْقَكُمْ سَبْعًا شِدَادًا"
},
{
    "id": "5685",
    "sura": "78",
    "VerseIDAr": "13",
    "ayat": "وَجَعَلْنَا سِرَاجًا وَهَّاجًا"
},
{
    "id": "5686",
    "sura": "78",
    "VerseIDAr": "14",
    "ayat": "وَأَنْزَلْنَا مِنَ الْمُعْصِرَاتِ مَاءً ثَجَّاجًا"
},
{
    "id": "5687",
    "sura": "78",
    "VerseIDAr": "15",
    "ayat": "لِنُخْرِجَ بِهِ حَبًّا وَنَبَاتًا"
},
{
    "id": "5688",
    "sura": "78",
    "VerseIDAr": "16",
    "ayat": "وَجَنَّاتٍ أَلْفَافًا"
},
{
    "id": "5689",
    "sura": "78",
    "VerseIDAr": "17",
    "ayat": "إِنَّ يَوْمَ الْفَصْلِ كَانَ مِيقَاتًا"
},
{
    "id": "5690",
    "sura": "78",
    "VerseIDAr": "18",
    "ayat": "يَوْمَ يُنْفَخُ فِي الصُّورِ فَتَأْتُونَ أَفْوَاجًا"
},
{
    "id": "5691",
    "sura": "78",
    "VerseIDAr": "19",
    "ayat": "وَفُتِحَتِ السَّمَاءُ فَكَانَتْ أَبْوَابًا"
},
{
    "id": "5692",
    "sura": "78",
    "VerseIDAr": "20",
    "ayat": "وَسُيِّرَتِ الْجِبَالُ فَكَانَتْ سَرَابًا"
},
{
    "id": "5693",
    "sura": "78",
    "VerseIDAr": "21",
    "ayat": "إِنَّ جَهَنَّمَ كَانَتْ مِرْصَادًا"
},
{
    "id": "5694",
    "sura": "78",
    "VerseIDAr": "22",
    "ayat": "لِلطَّاغِينَ مَآبًا"
},
{
    "id": "5695",
    "sura": "78",
    "VerseIDAr": "23",
    "ayat": "لَابِثِينَ فِيهَا أَحْقَابًا"
},
{
    "id": "5696",
    "sura": "78",
    "VerseIDAr": "24",
    "ayat": "لَا يَذُوقُونَ فِيهَا بَرْدًا وَلَا شَرَابًا"
},
{
    "id": "5697",
    "sura": "78",
    "VerseIDAr": "25",
    "ayat": "إِلَّا حَمِيمًا وَغَسَّاقًا"
},
{
    "id": "5698",
    "sura": "78",
    "VerseIDAr": "26",
    "ayat": "جَزَاءً وِفَاقًا"
},
{
    "id": "5699",
    "sura": "78",
    "VerseIDAr": "27",
    "ayat": "إِنَّهُمْ كَانُوا لَا يَرْجُونَ حِسَابًا"
},
{
    "id": "5700",
    "sura": "78",
    "VerseIDAr": "28",
    "ayat": "وَكَذَّبُوا بِآيَاتِنَا كِذَّابًا"
},
{
    "id": "5701",
    "sura": "78",
    "VerseIDAr": "29",
    "ayat": "وَكُلَّ شَيْءٍ أَحْصَيْنَاهُ كِتَابًا"
},
{
    "id": "5702",
    "sura": "78",
    "VerseIDAr": "30",
    "ayat": "فَذُوقُوا فَلَنْ نَزِيدَكُمْ إِلَّا عَذَابًا"
},
{
    "id": "5703",
    "sura": "78",
    "VerseIDAr": "31",
    "ayat": "إِنَّ لِلْمُتَّقِينَ مَفَازًا"
},
{
    "id": "5704",
    "sura": "78",
    "VerseIDAr": "32",
    "ayat": "حَدَائِقَ وَأَعْنَابًا"
},
{
    "id": "5705",
    "sura": "78",
    "VerseIDAr": "33",
    "ayat": "وَكَوَاعِبَ أَتْرَابًا"
},
{
    "id": "5706",
    "sura": "78",
    "VerseIDAr": "34",
    "ayat": "وَكَأْسًا دِهَاقًا"
},
{
    "id": "5707",
    "sura": "78",
    "VerseIDAr": "35",
    "ayat": "لَا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا كِذَّابًا"
},
{
    "id": "5708",
    "sura": "78",
    "VerseIDAr": "36",
    "ayat": "جَزَاءً مِنْ رَبِّكَ عَطَاءً حِسَابًا"
},
{
    "id": "5709",
    "sura": "78",
    "VerseIDAr": "37",
    "ayat": "رَبِّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا الرَّحْمَٰنِ ۖ لَا يَمْلِكُونَ مِنْهُ خِطَابًا"
},
{
    "id": "5710",
    "sura": "78",
    "VerseIDAr": "38",
    "ayat": "يَوْمَ يَقُومُ الرُّوحُ وَالْمَلَائِكَةُ صَفًّا ۖ لَا يَتَكَلَّمُونَ إِلَّا مَنْ أَذِنَ لَهُ الرَّحْمَٰنُ وَقَالَ صَوَابًا"
},
{
    "id": "5711",
    "sura": "78",
    "VerseIDAr": "39",
    "ayat": "ذَٰلِكَ الْيَوْمُ الْحَقُّ ۖ فَمَنْ شَاءَ اتَّخَذَ إِلَىٰ رَبِّهِ مَآبًا"
},
{
    "id": "5712",
    "sura": "78",
    "VerseIDAr": "40",
    "ayat": "إِنَّا أَنْذَرْنَاكُمْ عَذَابًا قَرِيبًا يَوْمَ يَنْظُرُ الْمَرْءُ مَا قَدَّمَتْ يَدَاهُ وَيَقُولُ الْكَافِرُ يَا لَيْتَنِي كُنْتُ تُرَابًا"
}
]