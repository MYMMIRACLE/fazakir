[
{
    "id": "6044",
    "sura": "91",
    "VerseIDAr": "1",
    "ayat": " وَالشَّمْسِ وَضُحَاهَا"
},
{
    "id": "6045",
    "sura": "91",
    "VerseIDAr": "2",
    "ayat": "وَالْقَمَرِ إِذَا تَلَاهَا"
},
{
    "id": "6046",
    "sura": "91",
    "VerseIDAr": "3",
    "ayat": "وَالنَّهَارِ إِذَا جَلَّاهَا"
},
{
    "id": "6047",
    "sura": "91",
    "VerseIDAr": "4",
    "ayat": "وَاللَّيْلِ إِذَا يَغْشَاهَا"
},
{
    "id": "6048",
    "sura": "91",
    "VerseIDAr": "5",
    "ayat": "وَالسَّمَاءِ وَمَا بَنَاهَا"
},
{
    "id": "6049",
    "sura": "91",
    "VerseIDAr": "6",
    "ayat": "وَالْأَرْضِ وَمَا طَحَاهَا"
},
{
    "id": "6050",
    "sura": "91",
    "VerseIDAr": "7",
    "ayat": "وَنَفْسٍ وَمَا سَوَّاهَا"
},
{
    "id": "6051",
    "sura": "91",
    "VerseIDAr": "8",
    "ayat": "فَأَلْهَمَهَا فُجُورَهَا وَتَقْوَاهَا"
},
{
    "id": "6052",
    "sura": "91",
    "VerseIDAr": "9",
    "ayat": "قَدْ أَفْلَحَ مَنْ زَكَّاهَا"
},
{
    "id": "6053",
    "sura": "91",
    "VerseIDAr": "10",
    "ayat": "وَقَدْ خَابَ مَنْ دَسَّاهَا"
},
{
    "id": "6054",
    "sura": "91",
    "VerseIDAr": "11",
    "ayat": "كَذَّبَتْ ثَمُودُ بِطَغْوَاهَا"
},
{
    "id": "6055",
    "sura": "91",
    "VerseIDAr": "12",
    "ayat": "إِذِ انْبَعَثَ أَشْقَاهَا"
},
{
    "id": "6056",
    "sura": "91",
    "VerseIDAr": "13",
    "ayat": "فَقَالَ لَهُمْ رَسُولُ اللَّهِ نَاقَةَ اللَّهِ وَسُقْيَاهَا"
},
{
    "id": "6057",
    "sura": "91",
    "VerseIDAr": "14",
    "ayat": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمْدَمَ عَلَيْهِمْ رَبُّهُمْ بِذَنْبِهِمْ فَسَوَّاهَا"
},
{
    "id": "6058",
    "sura": "91",
    "VerseIDAr": "15",
    "ayat": "وَلَا يَخَافُ عُقْبَاهَا"
}
]