[
{
    "id": "6024",
    "sura": "90",
    "VerseIDAr": "1",
    "ayat": " لَا أُقْسِمُ بِهَٰذَا الْبَلَدِ"
},
{
    "id": "6025",
    "sura": "90",
    "VerseIDAr": "2",
    "ayat": "وَأَنْتَ حِلٌّ بِهَٰذَا الْبَلَدِ"
},
{
    "id": "6026",
    "sura": "90",
    "VerseIDAr": "3",
    "ayat": "وَوَالِدٍ وَمَا وَلَدَ"
},
{
    "id": "6027",
    "sura": "90",
    "VerseIDAr": "4",
    "ayat": "لَقَدْ خَلَقْنَا الْإِنْسَانَ فِي كَبَدٍ"
},
{
    "id": "6028",
    "sura": "90",
    "VerseIDAr": "5",
    "ayat": "أَيَحْسَبُ أَنْ لَنْ يَقْدِرَ عَلَيْهِ أَحَدٌ"
},
{
    "id": "6029",
    "sura": "90",
    "VerseIDAr": "6",
    "ayat": "يَقُولُ أَهْلَكْتُ مَالًا لُبَدًا"
},
{
    "id": "6030",
    "sura": "90",
    "VerseIDAr": "7",
    "ayat": "أَيَحْسَبُ أَنْ لَمْ يَرَهُ أَحَدٌ"
},
{
    "id": "6031",
    "sura": "90",
    "VerseIDAr": "8",
    "ayat": "أَلَمْ نَجْعَلْ لَهُ عَيْنَيْنِ"
},
{
    "id": "6032",
    "sura": "90",
    "VerseIDAr": "9",
    "ayat": "وَلِسَانًا وَشَفَتَيْنِ"
},
{
    "id": "6033",
    "sura": "90",
    "VerseIDAr": "10",
    "ayat": "وَهَدَيْنَاهُ النَّجْدَيْنِ"
},
{
    "id": "6034",
    "sura": "90",
    "VerseIDAr": "11",
    "ayat": "فَلَا اقْتَحَمَ الْعَقَبَةَ"
},
{
    "id": "6035",
    "sura": "90",
    "VerseIDAr": "12",
    "ayat": "وَمَا أَدْرَاكَ مَا الْعَقَبَةُ"
},
{
    "id": "6036",
    "sura": "90",
    "VerseIDAr": "13",
    "ayat": "فَكُّ رَقَبَةٍ"
},
{
    "id": "6037",
    "sura": "90",
    "VerseIDAr": "14",
    "ayat": "أَوْ إِطْعَامٌ فِي يَوْمٍ ذِي مَسْغَبَةٍ"
},
{
    "id": "6038",
    "sura": "90",
    "VerseIDAr": "15",
    "ayat": "يَتِيمًا ذَا مَقْرَبَةٍ"
},
{
    "id": "6039",
    "sura": "90",
    "VerseIDAr": "16",
    "ayat": "أَوْ مِسْكِينًا ذَا مَتْرَبَةٍ"
},
{
    "id": "6040",
    "sura": "90",
    "VerseIDAr": "17",
    "ayat": "ثُمَّ كَانَ مِنَ الَّذِينَ آمَنُوا وَتَوَاصَوْا بِالصَّبْرِ وَتَوَاصَوْا بِالْمَرْحَمَةِ"
},
{
    "id": "6041",
    "sura": "90",
    "VerseIDAr": "18",
    "ayat": "أُولَٰئِكَ أَصْحَابُ الْمَيْمَنَةِ"
},
{
    "id": "6042",
    "sura": "90",
    "VerseIDAr": "19",
    "ayat": "وَالَّذِينَ كَفَرُوا بِآيَاتِنَا هُمْ أَصْحَابُ الْمَشْأَمَةِ"
},
{
    "id": "6043",
    "sura": "90",
    "VerseIDAr": "20",
    "ayat": "عَلَيْهِمْ نَارٌ مُؤْصَدَةٌ"
}
]