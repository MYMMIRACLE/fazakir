[
{
    "id": "5949",
    "sura": "87",
    "VerseIDAr": "1",
    "ayat": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ سَبِّحِ اسْمَ رَبِّكَ الْأَعْلَى"
},
{
    "id": "5950",
    "sura": "87",
    "VerseIDAr": "2",
    "ayat": "الَّذِي خَلَقَ فَسَوَّىٰ"
},
{
    "id": "5951",
    "sura": "87",
    "VerseIDAr": "3",
    "ayat": "وَالَّذِي قَدَّرَ فَهَدَىٰ"
},
{
    "id": "5952",
    "sura": "87",
    "VerseIDAr": "4",
    "ayat": "وَالَّذِي أَخْرَجَ الْمَرْعَىٰ"
},
{
    "id": "5953",
    "sura": "87",
    "VerseIDAr": "5",
    "ayat": "فَجَعَلَهُ غُثَاءً أَحْوَىٰ"
},
{
    "id": "5954",
    "sura": "87",
    "VerseIDAr": "6",
    "ayat": "سَنُقْرِئُكَ فَلَا تَنْسَىٰ"
},
{
    "id": "5955",
    "sura": "87",
    "VerseIDAr": "7",
    "ayat": "إِلَّا مَا شَاءَ اللَّهُ ۚ إِنَّهُ يَعْلَمُ الْجَهْرَ وَمَا يَخْفَىٰ"
},
{
    "id": "5956",
    "sura": "87",
    "VerseIDAr": "8",
    "ayat": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ"
},
{
    "id": "5957",
    "sura": "87",
    "VerseIDAr": "9",
    "ayat": "فَذَكِّرْ إِنْ نَفَعَتِ الذِّكْرَىٰ"
},
{
    "id": "5958",
    "sura": "87",
    "VerseIDAr": "10",
    "ayat": "سَيَذَّكَّرُ مَنْ يَخْشَىٰ"
},
{
    "id": "5959",
    "sura": "87",
    "VerseIDAr": "11",
    "ayat": "وَيَتَجَنَّبُهَا الْأَشْقَى"
},
{
    "id": "5960",
    "sura": "87",
    "VerseIDAr": "12",
    "ayat": "الَّذِي يَصْلَى النَّارَ الْكُبْرَىٰ"
},
{
    "id": "5961",
    "sura": "87",
    "VerseIDAr": "13",
    "ayat": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ"
},
{
    "id": "5962",
    "sura": "87",
    "VerseIDAr": "14",
    "ayat": "قَدْ أَفْلَحَ مَنْ تَزَكَّىٰ"
},
{
    "id": "5963",
    "sura": "87",
    "VerseIDAr": "15",
    "ayat": "وَذَكَرَ اسْمَ رَبِّهِ فَصَلَّىٰ"
},
{
    "id": "5964",
    "sura": "87",
    "VerseIDAr": "16",
    "ayat": "بَلْ تُؤْثِرُونَ الْحَيَاةَ الدُّنْيَا"
},
{
    "id": "5965",
    "sura": "87",
    "VerseIDAr": "17",
    "ayat": "وَالْآخِرَةُ خَيْرٌ وَأَبْقَىٰ"
},
{
    "id": "5966",
    "sura": "87",
    "VerseIDAr": "18",
    "ayat": "إِنَّ هَٰذَا لَفِي الصُّحُفِ الْأُولَىٰ"
},
{
    "id": "5967",
    "sura": "87",
    "VerseIDAr": "19",
    "ayat": "صُحُفِ إِبْرَاهِيمَ وَمُوسَىٰ"
}
]