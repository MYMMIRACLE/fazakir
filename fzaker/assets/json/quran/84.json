[
    {
        "id": "5885",
        "sura": "84",
        "VerseIDAr": "1",
        "ayat": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ إِذَا السَّمَاءُ انْشَقَّتْ"
    },
    {
        "id": "5886",
        "sura": "84",
        "VerseIDAr": "2",
        "ayat": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ"
    },
    {
        "id": "5887",
        "sura": "84",
        "VerseIDAr": "3",
        "ayat": "وَإِذَا الْأَرْضُ مُدَّتْ"
    },
    {
        "id": "5888",
        "sura": "84",
        "VerseIDAr": "4",
        "ayat": "وَأَلْقَتْ مَا فِيهَا وَتَخَلَّتْ"
    },
    {
        "id": "5889",
        "sura": "84",
        "VerseIDAr": "5",
        "ayat": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ"
    },
    {
        "id": "5890",
        "sura": "84",
        "VerseIDAr": "6",
        "ayat": "يَا أَيُّهَا الْإِنْسَانُ إِنَّكَ كَادِحٌ إِلَىٰ رَبِّكَ كَدْحًا فَمُلَاقِيهِ"
    },
    {
        "id": "5891",
        "sura": "84",
        "VerseIDAr": "7",
        "ayat": "فَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِيَمِينِهِ"
    },
    {
        "id": "5892",
        "sura": "84",
        "VerseIDAr": "8",
        "ayat": "فَسَوْفَ يُحَاسَبُ حِسَابًا يَسِيرًا"
    },
    {
        "id": "5893",
        "sura": "84",
        "VerseIDAr": "9",
        "ayat": "وَيَنْقَلِبُ إِلَىٰ أَهْلِهِ مَسْرُورًا"
    },
    {
        "id": "5894",
        "sura": "84",
        "VerseIDAr": "10",
        "ayat": "وَأَمَّا مَنْ أُوتِيَ كِتَابَهُ وَرَاءَ ظَهْرِهِ"
    },
    {
        "id": "5895",
        "sura": "84",
        "VerseIDAr": "11",
        "ayat": "فَسَوْفَ يَدْعُو ثُبُورًا"
    },
    {
        "id": "5896",
        "sura": "84",
        "VerseIDAr": "12",
        "ayat": "وَيَصْلَىٰ سَعِيرًا"
    },
    {
        "id": "5897",
        "sura": "84",
        "VerseIDAr": "13",
        "ayat": "إِنَّهُ كَانَ فِي أَهْلِهِ مَسْرُورًا"
    },
    {
        "id": "5898",
        "sura": "84",
        "VerseIDAr": "14",
        "ayat": "إِنَّهُ ظَنَّ أَنْ لَنْ يَحُورَ"
    },
    {
        "id": "5899",
        "sura": "84",
        "VerseIDAr": "15",
        "ayat": "بَلَىٰ إِنَّ رَبَّهُ كَانَ بِهِ بَصِيرًا"
    },
    {
        "id": "5900",
        "sura": "84",
        "VerseIDAr": "16",
        "ayat": "فَلَا أُقْسِمُ بِالشَّفَقِ"
    },
    {
        "id": "5901",
        "sura": "84",
        "VerseIDAr": "17",
        "ayat": "وَاللَّيْلِ وَمَا وَسَقَ"
    },
    {
        "id": "5902",
        "sura": "84",
        "VerseIDAr": "18",
        "ayat": "وَالْقَمَرِ إِذَا اتَّسَقَ"
    },
    {
        "id": "5903",
        "sura": "84",
        "VerseIDAr": "19",
        "ayat": "لَتَرْكَبُنَّ طَبَقًا عَنْ طَبَقٍ"
    },
    {
        "id": "5904",
        "sura": "84",
        "VerseIDAr": "20",
        "ayat": "فَمَا لَهُمْ لَا يُؤْمِنُونَ"
    },
    {
        "id": "5905",
        "sura": "84",
        "VerseIDAr": "21",
        "ayat": "وَإِذَا قُرِئَ عَلَيْهِمُ الْقُرْآنُ لَا يَسْجُدُونَ ۩"
    },
    {
        "id": "5906",
        "sura": "84",
        "VerseIDAr": "22",
        "ayat": "بَلِ الَّذِينَ كَفَرُوا يُكَذِّبُونَ"
    },
    {
        "id": "5907",
        "sura": "84",
        "VerseIDAr": "23",
        "ayat": "وَاللَّهُ أَعْلَمُ بِمَا يُوعُونَ"
    },
    {
        "id": "5908",
        "sura": "84",
        "VerseIDAr": "24",
        "ayat": "فَبَشِّرْهُمْ بِعَذَابٍ أَلِيمٍ"
    },
    {
        "id": "5909",
        "sura": "84",
        "VerseIDAr": "25",
        "ayat": "إِلَّا الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍ"
    }
]