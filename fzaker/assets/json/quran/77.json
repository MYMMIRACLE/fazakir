[
{
    "id": "5623",
    "sura": "77",
    "VerseIDAr": "1",
    "ayat": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَالْمُرْسَلَاتِ عُرْفًا"
},
{
    "id": "5624",
    "sura": "77",
    "VerseIDAr": "2",
    "ayat": "فَالْعَاصِفَاتِ عَصْفًا"
},
{
    "id": "5625",
    "sura": "77",
    "VerseIDAr": "3",
    "ayat": "وَالنَّاشِرَاتِ نَشْرًا"
},
{
    "id": "5626",
    "sura": "77",
    "VerseIDAr": "4",
    "ayat": "فَالْفَارِقَاتِ فَرْقًا"
},
{
    "id": "5627",
    "sura": "77",
    "VerseIDAr": "5",
    "ayat": "فَالْمُلْقِيَاتِ ذِكْرًا"
},
{
    "id": "5628",
    "sura": "77",
    "VerseIDAr": "6",
    "ayat": "عُذْرًا أَوْ نُذْرًا"
},
{
    "id": "5629",
    "sura": "77",
    "VerseIDAr": "7",
    "ayat": "إِنَّمَا تُوعَدُونَ لَوَاقِعٌ"
},
{
    "id": "5630",
    "sura": "77",
    "VerseIDAr": "8",
    "ayat": "فَإِذَا النُّجُومُ طُمِسَتْ"
},
{
    "id": "5631",
    "sura": "77",
    "VerseIDAr": "9",
    "ayat": "وَإِذَا السَّمَاءُ فُرِجَتْ"
},
{
    "id": "5632",
    "sura": "77",
    "VerseIDAr": "10",
    "ayat": "وَإِذَا الْجِبَالُ نُسِفَتْ"
},
{
    "id": "5633",
    "sura": "77",
    "VerseIDAr": "11",
    "ayat": "وَإِذَا الرُّسُلُ أُقِّتَتْ"
},
{
    "id": "5634",
    "sura": "77",
    "VerseIDAr": "12",
    "ayat": "لِأَيِّ يَوْمٍ أُجِّلَتْ"
},
{
    "id": "5635",
    "sura": "77",
    "VerseIDAr": "13",
    "ayat": "لِيَوْمِ الْفَصْلِ"
},
{
    "id": "5636",
    "sura": "77",
    "VerseIDAr": "14",
    "ayat": "وَمَا أَدْرَاكَ مَا يَوْمُ الْفَصْلِ"
},
{
    "id": "5637",
    "sura": "77",
    "VerseIDAr": "15",
    "ayat": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
},
{
    "id": "5638",
    "sura": "77",
    "VerseIDAr": "16",
    "ayat": "أَلَمْ نُهْلِكِ الْأَوَّلِينَ"
},
{
    "id": "5639",
    "sura": "77",
    "VerseIDAr": "17",
    "ayat": "ثُمَّ نُتْبِعُهُمُ الْآخِرِينَ"
},
{
    "id": "5640",
    "sura": "77",
    "VerseIDAr": "18",
    "ayat": "كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَ"
},
{
    "id": "5641",
    "sura": "77",
    "VerseIDAr": "19",
    "ayat": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
},
{
    "id": "5642",
    "sura": "77",
    "VerseIDAr": "20",
    "ayat": "أَلَمْ نَخْلُقْكُمْ مِنْ مَاءٍ مَهِينٍ"
},
{
    "id": "5643",
    "sura": "77",
    "VerseIDAr": "21",
    "ayat": "فَجَعَلْنَاهُ فِي قَرَارٍ مَكِينٍ"
},
{
    "id": "5644",
    "sura": "77",
    "VerseIDAr": "22",
    "ayat": "إِلَىٰ قَدَرٍ مَعْلُومٍ"
},
{
    "id": "5645",
    "sura": "77",
    "VerseIDAr": "23",
    "ayat": "فَقَدَرْنَا فَنِعْمَ الْقَادِرُونَ"
},
{
    "id": "5646",
    "sura": "77",
    "VerseIDAr": "24",
    "ayat": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
},
{
    "id": "5647",
    "sura": "77",
    "VerseIDAr": "25",
    "ayat": "أَلَمْ نَجْعَلِ الْأَرْضَ كِفَاتًا"
},
{
    "id": "5648",
    "sura": "77",
    "VerseIDAr": "26",
    "ayat": "أَحْيَاءً وَأَمْوَاتًا"
},
{
    "id": "5649",
    "sura": "77",
    "VerseIDAr": "27",
    "ayat": "وَجَعَلْنَا فِيهَا رَوَاسِيَ شَامِخَاتٍ وَأَسْقَيْنَاكُمْ مَاءً فُرَاتًا"
},
{
    "id": "5650",
    "sura": "77",
    "VerseIDAr": "28",
    "ayat": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
},
{
    "id": "5651",
    "sura": "77",
    "VerseIDAr": "29",
    "ayat": "انْطَلِقُوا إِلَىٰ مَا كُنْتُمْ بِهِ تُكَذِّبُونَ"
},
{
    "id": "5652",
    "sura": "77",
    "VerseIDAr": "30",
    "ayat": "انْطَلِقُوا إِلَىٰ ظِلٍّ ذِي ثَلَاثِ شُعَبٍ"
},
{
    "id": "5653",
    "sura": "77",
    "VerseIDAr": "31",
    "ayat": "لَا ظَلِيلٍ وَلَا يُغْنِي مِنَ اللَّهَبِ"
},
{
    "id": "5654",
    "sura": "77",
    "VerseIDAr": "32",
    "ayat": "إِنَّهَا تَرْمِي بِشَرَرٍ كَالْقَصْرِ"
},
{
    "id": "5655",
    "sura": "77",
    "VerseIDAr": "33",
    "ayat": "كَأَنَّهُ جِمَالَتٌ صُفْرٌ"
},
{
    "id": "5656",
    "sura": "77",
    "VerseIDAr": "34",
    "ayat": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
},
{
    "id": "5657",
    "sura": "77",
    "VerseIDAr": "35",
    "ayat": "هَٰذَا يَوْمُ لَا يَنْطِقُونَ"
},
{
    "id": "5658",
    "sura": "77",
    "VerseIDAr": "36",
    "ayat": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ"
},
{
    "id": "5659",
    "sura": "77",
    "VerseIDAr": "37",
    "ayat": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
},
{
    "id": "5660",
    "sura": "77",
    "VerseIDAr": "38",
    "ayat": "هَٰذَا يَوْمُ الْفَصْلِ ۖ جَمَعْنَاكُمْ وَالْأَوَّلِينَ"
},
{
    "id": "5661",
    "sura": "77",
    "VerseIDAr": "39",
    "ayat": "فَإِنْ كَانَ لَكُمْ كَيْدٌ فَكِيدُونِ"
},
{
    "id": "5662",
    "sura": "77",
    "VerseIDAr": "40",
    "ayat": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
},
{
    "id": "5663",
    "sura": "77",
    "VerseIDAr": "41",
    "ayat": "إِنَّ الْمُتَّقِينَ فِي ظِلَالٍ وَعُيُونٍ"
},
{
    "id": "5664",
    "sura": "77",
    "VerseIDAr": "42",
    "ayat": "وَفَوَاكِهَ مِمَّا يَشْتَهُونَ"
},
{
    "id": "5665",
    "sura": "77",
    "VerseIDAr": "43",
    "ayat": "كُلُوا وَاشْرَبُوا هَنِيئًا بِمَا كُنْتُمْ تَعْمَلُونَ"
},
{
    "id": "5666",
    "sura": "77",
    "VerseIDAr": "44",
    "ayat": "إِنَّا كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ"
},
{
    "id": "5667",
    "sura": "77",
    "VerseIDAr": "45",
    "ayat": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
},
{
    "id": "5668",
    "sura": "77",
    "VerseIDAr": "46",
    "ayat": "كُلُوا وَتَمَتَّعُوا قَلِيلًا إِنَّكُمْ مُجْرِمُونَ"
},
{
    "id": "5669",
    "sura": "77",
    "VerseIDAr": "47",
    "ayat": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
},
{
    "id": "5670",
    "sura": "77",
    "VerseIDAr": "48",
    "ayat": "وَإِذَا قِيلَ لَهُمُ ارْكَعُوا لَا يَرْكَعُونَ"
},
{
    "id": "5671",
    "sura": "77",
    "VerseIDAr": "49",
    "ayat": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
},
{
    "id": "5672",
    "sura": "77",
    "VerseIDAr": "50",
    "ayat": "فَبِأَيِّ حَدِيثٍ بَعْدَهُ يُؤْمِنُونَ"
}
]