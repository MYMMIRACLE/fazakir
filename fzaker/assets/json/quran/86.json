[
{
    "id": "5932",
    "sura": "86",
    "VerseIDAr": "1",
    "ayat": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَالسَّمَاءِ وَالطَّارِقِ"
},
{
    "id": "5933",
    "sura": "86",
    "VerseIDAr": "2",
    "ayat": "وَمَا أَدْرَاكَ مَا الطَّارِقُ"
},
{
    "id": "5934",
    "sura": "86",
    "VerseIDAr": "3",
    "ayat": "النَّجْمُ الثَّاقِبُ"
},
{
    "id": "5935",
    "sura": "86",
    "VerseIDAr": "4",
    "ayat": "إِنْ كُلُّ نَفْسٍ لَمَّا عَلَيْهَا حَافِظٌ"
},
{
    "id": "5936",
    "sura": "86",
    "VerseIDAr": "5",
    "ayat": "فَلْيَنْظُرِ الْإِنْسَانُ مِمَّ خُلِقَ"
},
{
    "id": "5937",
    "sura": "86",
    "VerseIDAr": "6",
    "ayat": "خُلِقَ مِنْ مَاءٍ دَافِقٍ"
},
{
    "id": "5938",
    "sura": "86",
    "VerseIDAr": "7",
    "ayat": "يَخْرُجُ مِنْ بَيْنِ الصُّلْبِ وَالتَّرَائِبِ"
},
{
    "id": "5939",
    "sura": "86",
    "VerseIDAr": "8",
    "ayat": "إِنَّهُ عَلَىٰ رَجْعِهِ لَقَادِرٌ"
},
{
    "id": "5940",
    "sura": "86",
    "VerseIDAr": "9",
    "ayat": "يَوْمَ تُبْلَى السَّرَائِرُ"
},
{
    "id": "5941",
    "sura": "86",
    "VerseIDAr": "10",
    "ayat": "فَمَا لَهُ مِنْ قُوَّةٍ وَلَا نَاصِرٍ"
},
{
    "id": "5942",
    "sura": "86",
    "VerseIDAr": "11",
    "ayat": "وَالسَّمَاءِ ذَاتِ الرَّجْعِ"
},
{
    "id": "5943",
    "sura": "86",
    "VerseIDAr": "12",
    "ayat": "وَالْأَرْضِ ذَاتِ الصَّدْعِ"
},
{
    "id": "5944",
    "sura": "86",
    "VerseIDAr": "13",
    "ayat": "إِنَّهُ لَقَوْلٌ فَصْلٌ"
},
{
    "id": "5945",
    "sura": "86",
    "VerseIDAr": "14",
    "ayat": "وَمَا هُوَ بِالْهَزْلِ"
},
{
    "id": "5946",
    "sura": "86",
    "VerseIDAr": "15",
    "ayat": "إِنَّهُمْ يَكِيدُونَ كَيْدًا"
},
{
    "id": "5947",
    "sura": "86",
    "VerseIDAr": "16",
    "ayat": "وَأَكِيدُ كَيْدًا"
},
{
    "id": "5948",
    "sura": "86",
    "VerseIDAr": "17",
    "ayat": "فَمَهِّلِ الْكَافِرِينَ أَمْهِلْهُمْ رُوَيْدًا"
}
]