[
{
    "id": "2933",
    "sura": "26",
    "VerseIDAr": "1",
    "ayat": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ طسم"
},
{
    "id": "2934",
    "sura": "26",
    "VerseIDAr": "2",
    "ayat": "تِلْكَ آيَاتُ الْكِتَابِ الْمُبِينِ"
},
{
    "id": "2935",
    "sura": "26",
    "VerseIDAr": "3",
    "ayat": "لَعَلَّكَ بَاخِعٌ نَفْسَكَ أَلَّا يَكُونُوا مُؤْمِنِينَ"
},
{
    "id": "2936",
    "sura": "26",
    "VerseIDAr": "4",
    "ayat": "إِنْ نَشَأْ نُنَزِّلْ عَلَيْهِمْ مِنَ السَّمَاءِ آيَةً فَظَلَّتْ أَعْنَاقُهُمْ لَهَا خَاضِعِينَ"
},
{
    "id": "2937",
    "sura": "26",
    "VerseIDAr": "5",
    "ayat": "وَمَا يَأْتِيهِمْ مِنْ ذِكْرٍ مِنَ الرَّحْمَٰنِ مُحْدَثٍ إِلَّا كَانُوا عَنْهُ مُعْرِضِينَ"
},
{
    "id": "2938",
    "sura": "26",
    "VerseIDAr": "6",
    "ayat": "فَقَدْ كَذَّبُوا فَسَيَأْتِيهِمْ أَنْبَاءُ مَا كَانُوا بِهِ يَسْتَهْزِئُونَ"
},
{
    "id": "2939",
    "sura": "26",
    "VerseIDAr": "7",
    "ayat": "أَوَلَمْ يَرَوْا إِلَى الْأَرْضِ كَمْ أَنْبَتْنَا فِيهَا مِنْ كُلِّ زَوْجٍ كَرِيمٍ"
},
{
    "id": "2940",
    "sura": "26",
    "VerseIDAr": "8",
    "ayat": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُمْ مُؤْمِنِينَ"
},
{
    "id": "2941",
    "sura": "26",
    "VerseIDAr": "9",
    "ayat": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
},
{
    "id": "2942",
    "sura": "26",
    "VerseIDAr": "10",
    "ayat": "وَإِذْ نَادَىٰ رَبُّكَ مُوسَىٰ أَنِ ائْتِ الْقَوْمَ الظَّالِمِينَ"
},
{
    "id": "2943",
    "sura": "26",
    "VerseIDAr": "11",
    "ayat": "قَوْمَ فِرْعَوْنَ ۚ أَلَا يَتَّقُونَ"
},
{
    "id": "2944",
    "sura": "26",
    "VerseIDAr": "12",
    "ayat": "قَالَ رَبِّ إِنِّي أَخَافُ أَنْ يُكَذِّبُونِ"
},
{
    "id": "2945",
    "sura": "26",
    "VerseIDAr": "13",
    "ayat": "وَيَضِيقُ صَدْرِي وَلَا يَنْطَلِقُ لِسَانِي فَأَرْسِلْ إِلَىٰ هَارُونَ"
},
{
    "id": "2946",
    "sura": "26",
    "VerseIDAr": "14",
    "ayat": "وَلَهُمْ عَلَيَّ ذَنْبٌ فَأَخَافُ أَنْ يَقْتُلُونِ"
},
{
    "id": "2947",
    "sura": "26",
    "VerseIDAr": "15",
    "ayat": "قَالَ كَلَّا ۖ فَاذْهَبَا بِآيَاتِنَا ۖ إِنَّا مَعَكُمْ مُسْتَمِعُونَ"
},
{
    "id": "2948",
    "sura": "26",
    "VerseIDAr": "16",
    "ayat": "فَأْتِيَا فِرْعَوْنَ فَقُولَا إِنَّا رَسُولُ رَبِّ الْعَالَمِينَ"
},
{
    "id": "2949",
    "sura": "26",
    "VerseIDAr": "17",
    "ayat": "أَنْ أَرْسِلْ مَعَنَا بَنِي إِسْرَائِيلَ"
},
{
    "id": "2950",
    "sura": "26",
    "VerseIDAr": "18",
    "ayat": "قَالَ أَلَمْ نُرَبِّكَ فِينَا وَلِيدًا وَلَبِثْتَ فِينَا مِنْ عُمُرِكَ سِنِينَ"
},
{
    "id": "2951",
    "sura": "26",
    "VerseIDAr": "19",
    "ayat": "وَفَعَلْتَ فَعْلَتَكَ الَّتِي فَعَلْتَ وَأَنْتَ مِنَ الْكَافِرِينَ"
},
{
    "id": "2952",
    "sura": "26",
    "VerseIDAr": "20",
    "ayat": "قَالَ فَعَلْتُهَا إِذًا وَأَنَا مِنَ الضَّالِّينَ"
},
{
    "id": "2953",
    "sura": "26",
    "VerseIDAr": "21",
    "ayat": "فَفَرَرْتُ مِنْكُمْ لَمَّا خِفْتُكُمْ فَوَهَبَ لِي رَبِّي حُكْمًا وَجَعَلَنِي مِنَ الْمُرْسَلِينَ"
},
{
    "id": "2954",
    "sura": "26",
    "VerseIDAr": "22",
    "ayat": "وَتِلْكَ نِعْمَةٌ تَمُنُّهَا عَلَيَّ أَنْ عَبَّدْتَ بَنِي إِسْرَائِيلَ"
},
{
    "id": "2955",
    "sura": "26",
    "VerseIDAr": "23",
    "ayat": "قَالَ فِرْعَوْنُ وَمَا رَبُّ الْعَالَمِينَ"
},
{
    "id": "2956",
    "sura": "26",
    "VerseIDAr": "24",
    "ayat": "قَالَ رَبُّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا ۖ إِنْ كُنْتُمْ مُوقِنِينَ"
},
{
    "id": "2957",
    "sura": "26",
    "VerseIDAr": "25",
    "ayat": "قَالَ لِمَنْ حَوْلَهُ أَلَا تَسْتَمِعُونَ"
},
{
    "id": "2958",
    "sura": "26",
    "VerseIDAr": "26",
    "ayat": "قَالَ رَبُّكُمْ وَرَبُّ آبَائِكُمُ الْأَوَّلِينَ"
},
{
    "id": "2959",
    "sura": "26",
    "VerseIDAr": "27",
    "ayat": "قَالَ إِنَّ رَسُولَكُمُ الَّذِي أُرْسِلَ إِلَيْكُمْ لَمَجْنُونٌ"
},
{
    "id": "2960",
    "sura": "26",
    "VerseIDAr": "28",
    "ayat": "قَالَ رَبُّ الْمَشْرِقِ وَالْمَغْرِبِ وَمَا بَيْنَهُمَا ۖ إِنْ كُنْتُمْ تَعْقِلُونَ"
},
{
    "id": "2961",
    "sura": "26",
    "VerseIDAr": "29",
    "ayat": "قَالَ لَئِنِ اتَّخَذْتَ إِلَٰهًا غَيْرِي لَأَجْعَلَنَّكَ مِنَ الْمَسْجُونِينَ"
},
{
    "id": "2962",
    "sura": "26",
    "VerseIDAr": "30",
    "ayat": "قَالَ أَوَلَوْ جِئْتُكَ بِشَيْءٍ مُبِينٍ"
},
{
    "id": "2963",
    "sura": "26",
    "VerseIDAr": "31",
    "ayat": "قَالَ فَأْتِ بِهِ إِنْ كُنْتَ مِنَ الصَّادِقِينَ"
},
{
    "id": "2964",
    "sura": "26",
    "VerseIDAr": "32",
    "ayat": "فَأَلْقَىٰ عَصَاهُ فَإِذَا هِيَ ثُعْبَانٌ مُبِينٌ"
},
{
    "id": "2965",
    "sura": "26",
    "VerseIDAr": "33",
    "ayat": "وَنَزَعَ يَدَهُ فَإِذَا هِيَ بَيْضَاءُ لِلنَّاظِرِينَ"
},
{
    "id": "2966",
    "sura": "26",
    "VerseIDAr": "34",
    "ayat": "قَالَ لِلْمَلَإِ حَوْلَهُ إِنَّ هَٰذَا لَسَاحِرٌ عَلِيمٌ"
},
{
    "id": "2967",
    "sura": "26",
    "VerseIDAr": "35",
    "ayat": "يُرِيدُ أَنْ يُخْرِجَكُمْ مِنْ أَرْضِكُمْ بِسِحْرِهِ فَمَاذَا تَأْمُرُونَ"
},
{
    "id": "2968",
    "sura": "26",
    "VerseIDAr": "36",
    "ayat": "قَالُوا أَرْجِهْ وَأَخَاهُ وَابْعَثْ فِي الْمَدَائِنِ حَاشِرِينَ"
},
{
    "id": "2969",
    "sura": "26",
    "VerseIDAr": "37",
    "ayat": "يَأْتُوكَ بِكُلِّ سَحَّارٍ عَلِيمٍ"
},
{
    "id": "2970",
    "sura": "26",
    "VerseIDAr": "38",
    "ayat": "فَجُمِعَ السَّحَرَةُ لِمِيقَاتِ يَوْمٍ مَعْلُومٍ"
},
{
    "id": "2971",
    "sura": "26",
    "VerseIDAr": "39",
    "ayat": "وَقِيلَ لِلنَّاسِ هَلْ أَنْتُمْ مُجْتَمِعُونَ"
},
{
    "id": "2972",
    "sura": "26",
    "VerseIDAr": "40",
    "ayat": "لَعَلَّنَا نَتَّبِعُ السَّحَرَةَ إِنْ كَانُوا هُمُ الْغَالِبِينَ"
},
{
    "id": "2973",
    "sura": "26",
    "VerseIDAr": "41",
    "ayat": "فَلَمَّا جَاءَ السَّحَرَةُ قَالُوا لِفِرْعَوْنَ أَئِنَّ لَنَا لَأَجْرًا إِنْ كُنَّا نَحْنُ الْغَالِبِينَ"
},
{
    "id": "2974",
    "sura": "26",
    "VerseIDAr": "42",
    "ayat": "قَالَ نَعَمْ وَإِنَّكُمْ إِذًا لَمِنَ الْمُقَرَّبِينَ"
},
{
    "id": "2975",
    "sura": "26",
    "VerseIDAr": "43",
    "ayat": "قَالَ لَهُمْ مُوسَىٰ أَلْقُوا مَا أَنْتُمْ مُلْقُونَ"
},
{
    "id": "2976",
    "sura": "26",
    "VerseIDAr": "44",
    "ayat": "فَأَلْقَوْا حِبَالَهُمْ وَعِصِيَّهُمْ وَقَالُوا بِعِزَّةِ فِرْعَوْنَ إِنَّا لَنَحْنُ الْغَالِبُونَ"
},
{
    "id": "2977",
    "sura": "26",
    "VerseIDAr": "45",
    "ayat": "فَأَلْقَىٰ مُوسَىٰ عَصَاهُ فَإِذَا هِيَ تَلْقَفُ مَا يَأْفِكُونَ"
},
{
    "id": "2978",
    "sura": "26",
    "VerseIDAr": "46",
    "ayat": "فَأُلْقِيَ السَّحَرَةُ سَاجِدِينَ"
},
{
    "id": "2979",
    "sura": "26",
    "VerseIDAr": "47",
    "ayat": "قَالُوا آمَنَّا بِرَبِّ الْعَالَمِينَ"
},
{
    "id": "2980",
    "sura": "26",
    "VerseIDAr": "48",
    "ayat": "رَبِّ مُوسَىٰ وَهَارُونَ"
},
{
    "id": "2981",
    "sura": "26",
    "VerseIDAr": "49",
    "ayat": "قَالَ آمَنْتُمْ لَهُ قَبْلَ أَنْ آذَنَ لَكُمْ ۖ إِنَّهُ لَكَبِيرُكُمُ الَّذِي عَلَّمَكُمُ السِّحْرَ فَلَسَوْفَ تَعْلَمُونَ ۚ لَأُقَطِّعَنَّ أَيْدِيَكُمْ وَأَرْجُلَكُمْ مِنْ خِلَافٍ وَلَأُصَلِّبَنَّكُمْ أَجْمَعِينَ"
},
{
    "id": "2982",
    "sura": "26",
    "VerseIDAr": "50",
    "ayat": "قَالُوا لَا ضَيْرَ ۖ إِنَّا إِلَىٰ رَبِّنَا مُنْقَلِبُونَ"
},
{
    "id": "2983",
    "sura": "26",
    "VerseIDAr": "51",
    "ayat": "إِنَّا نَطْمَعُ أَنْ يَغْفِرَ لَنَا رَبُّنَا خَطَايَانَا أَنْ كُنَّا أَوَّلَ الْمُؤْمِنِينَ"
},
{
    "id": "2984",
    "sura": "26",
    "VerseIDAr": "52",
    "ayat": "وَأَوْحَيْنَا إِلَىٰ مُوسَىٰ أَنْ أَسْرِ بِعِبَادِي إِنَّكُمْ مُتَّبَعُونَ"
},
{
    "id": "2985",
    "sura": "26",
    "VerseIDAr": "53",
    "ayat": "فَأَرْسَلَ فِرْعَوْنُ فِي الْمَدَائِنِ حَاشِرِينَ"
},
{
    "id": "2986",
    "sura": "26",
    "VerseIDAr": "54",
    "ayat": "إِنَّ هَٰؤُلَاءِ لَشِرْذِمَةٌ قَلِيلُونَ"
},
{
    "id": "2987",
    "sura": "26",
    "VerseIDAr": "55",
    "ayat": "وَإِنَّهُمْ لَنَا لَغَائِظُونَ"
},
{
    "id": "2988",
    "sura": "26",
    "VerseIDAr": "56",
    "ayat": "وَإِنَّا لَجَمِيعٌ حَاذِرُونَ"
},
{
    "id": "2989",
    "sura": "26",
    "VerseIDAr": "57",
    "ayat": "فَأَخْرَجْنَاهُمْ مِنْ جَنَّاتٍ وَعُيُونٍ"
},
{
    "id": "2990",
    "sura": "26",
    "VerseIDAr": "58",
    "ayat": "وَكُنُوزٍ وَمَقَامٍ كَرِيمٍ"
},
{
    "id": "2991",
    "sura": "26",
    "VerseIDAr": "59",
    "ayat": "كَذَٰلِكَ وَأَوْرَثْنَاهَا بَنِي إِسْرَائِيلَ"
},
{
    "id": "2992",
    "sura": "26",
    "VerseIDAr": "60",
    "ayat": "فَأَتْبَعُوهُمْ مُشْرِقِينَ"
},
{
    "id": "2993",
    "sura": "26",
    "VerseIDAr": "61",
    "ayat": "فَلَمَّا تَرَاءَى الْجَمْعَانِ قَالَ أَصْحَابُ مُوسَىٰ إِنَّا لَمُدْرَكُونَ"
},
{
    "id": "2994",
    "sura": "26",
    "VerseIDAr": "62",
    "ayat": "قَالَ كَلَّا ۖ إِنَّ مَعِيَ رَبِّي سَيَهْدِينِ"
},
{
    "id": "2995",
    "sura": "26",
    "VerseIDAr": "63",
    "ayat": "فَأَوْحَيْنَا إِلَىٰ مُوسَىٰ أَنِ اضْرِبْ بِعَصَاكَ الْبَحْرَ ۖ فَانْفَلَقَ فَكَانَ كُلُّ فِرْقٍ كَالطَّوْدِ الْعَظِيمِ"
},
{
    "id": "2996",
    "sura": "26",
    "VerseIDAr": "64",
    "ayat": "وَأَزْلَفْنَا ثَمَّ الْآخَرِينَ"
},
{
    "id": "2997",
    "sura": "26",
    "VerseIDAr": "65",
    "ayat": "وَأَنْجَيْنَا مُوسَىٰ وَمَنْ مَعَهُ أَجْمَعِينَ"
},
{
    "id": "2998",
    "sura": "26",
    "VerseIDAr": "66",
    "ayat": "ثُمَّ أَغْرَقْنَا الْآخَرِينَ"
},
{
    "id": "2999",
    "sura": "26",
    "VerseIDAr": "67",
    "ayat": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُمْ مُؤْمِنِينَ"
},
{
    "id": "3000",
    "sura": "26",
    "VerseIDAr": "68",
    "ayat": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
},
{
    "id": "3001",
    "sura": "26",
    "VerseIDAr": "69",
    "ayat": "وَاتْلُ عَلَيْهِمْ نَبَأَ إِبْرَاهِيمَ"
},
{
    "id": "3002",
    "sura": "26",
    "VerseIDAr": "70",
    "ayat": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِ مَا تَعْبُدُونَ"
},
{
    "id": "3003",
    "sura": "26",
    "VerseIDAr": "71",
    "ayat": "قَالُوا نَعْبُدُ أَصْنَامًا فَنَظَلُّ لَهَا عَاكِفِينَ"
},
{
    "id": "3004",
    "sura": "26",
    "VerseIDAr": "72",
    "ayat": "قَالَ هَلْ يَسْمَعُونَكُمْ إِذْ تَدْعُونَ"
},
{
    "id": "3005",
    "sura": "26",
    "VerseIDAr": "73",
    "ayat": "أَوْ يَنْفَعُونَكُمْ أَوْ يَضُرُّونَ"
},
{
    "id": "3006",
    "sura": "26",
    "VerseIDAr": "74",
    "ayat": "قَالُوا بَلْ وَجَدْنَا آبَاءَنَا كَذَٰلِكَ يَفْعَلُونَ"
