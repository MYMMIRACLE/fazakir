    "ayat": "فَأَخَذَهُمُ الْعَذَابُ ۗ إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُمْ مُؤْمِنِينَ"
},
{
    "id": "3091",
    "sura": "26",
    "VerseIDAr": "159",
    "ayat": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
},
{
    "id": "3092",
    "sura": "26",
    "VerseIDAr": "160",
    "ayat": "كَذَّبَتْ قَوْمُ لُوطٍ الْمُرْسَلِينَ"
},
{
    "id": "3093",
    "sura": "26",
    "VerseIDAr": "161",
    "ayat": "إِذْ قَالَ لَهُمْ أَخُوهُمْ لُوطٌ أَلَا تَتَّقُونَ"
},
{
    "id": "3094",
    "sura": "26",
    "VerseIDAr": "162",
    "ayat": "إِنِّي لَكُمْ رَسُولٌ أَمِينٌ"
},
{
    "id": "3095",
    "sura": "26",
    "VerseIDAr": "163",
    "ayat": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
},
{
    "id": "3096",
    "sura": "26",
    "VerseIDAr": "164",
    "ayat": "وَمَا أَسْأَلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِيَ إِلَّا عَلَىٰ رَبِّ الْعَالَمِينَ"
},
{
    "id": "3097",
    "sura": "26",
    "VerseIDAr": "165",
    "ayat": "أَتَأْتُونَ الذُّكْرَانَ مِنَ الْعَالَمِينَ"
},
{
    "id": "3098",
    "sura": "26",
    "VerseIDAr": "166",
    "ayat": "وَتَذَرُونَ مَا خَلَقَ لَكُمْ رَبُّكُمْ مِنْ أَزْوَاجِكُمْ ۚ بَلْ أَنْتُمْ قَوْمٌ عَادُونَ"
},
{
    "id": "3099",
    "sura": "26",
    "VerseIDAr": "167",
    "ayat": "قَالُوا لَئِنْ لَمْ تَنْتَهِ يَا لُوطُ لَتَكُونَنَّ مِنَ الْمُخْرَجِينَ"
},
{
    "id": "3100",
    "sura": "26",
    "VerseIDAr": "168",
    "ayat": "قَالَ إِنِّي لِعَمَلِكُمْ مِنَ الْقَالِينَ"
},
{
    "id": "3101",
    "sura": "26",
    "VerseIDAr": "169",
    "ayat": "رَبِّ نَجِّنِي وَأَهْلِي مِمَّا يَعْمَلُونَ"
},
{
    "id": "3102",
    "sura": "26",
    "VerseIDAr": "170",
    "ayat": "فَنَجَّيْنَاهُ وَأَهْلَهُ أَجْمَعِينَ"
},
{
    "id": "3103",
    "sura": "26",
    "VerseIDAr": "171",
    "ayat": "إِلَّا عَجُوزًا فِي الْغَابِرِينَ"
},
{
    "id": "3104",
    "sura": "26",
    "VerseIDAr": "172",
    "ayat": "ثُمَّ دَمَّرْنَا الْآخَرِينَ"
},
{
    "id": "3105",
    "sura": "26",
    "VerseIDAr": "173",
    "ayat": "وَأَمْطَرْنَا عَلَيْهِمْ مَطَرًا ۖ فَسَاءَ مَطَرُ الْمُنْذَرِينَ"
},
{
    "id": "3106",
    "sura": "26",
    "VerseIDAr": "174",
    "ayat": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُمْ مُؤْمِنِينَ"
},
{
    "id": "3107",
    "sura": "26",
    "VerseIDAr": "175",
    "ayat": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
},
{
    "id": "3108",
    "sura": "26",
    "VerseIDAr": "176",
    "ayat": "كَذَّبَ أَصْحَابُ الْأَيْكَةِ الْمُرْسَلِينَ"
},
{
    "id": "3109",
    "sura": "26",
    "VerseIDAr": "177",
    "ayat": "إِذْ قَالَ لَهُمْ شُعَيْبٌ أَلَا تَتَّقُونَ"
},
{
    "id": "3110",
    "sura": "26",
    "VerseIDAr": "178",
    "ayat": "إِنِّي لَكُمْ رَسُولٌ أَمِينٌ"
},
{
    "id": "3111",
    "sura": "26",
    "VerseIDAr": "179",
    "ayat": "فَاتَّقُوا اللَّهَ وَأَطِيعُونِ"
},
{
    "id": "3112",
    "sura": "26",
    "VerseIDAr": "180",
    "ayat": "وَمَا أَسْأَلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِيَ إِلَّا عَلَىٰ رَبِّ الْعَالَمِينَ"
},
{
    "id": "3113",
    "sura": "26",
    "VerseIDAr": "181",
    "ayat": "أَوْفُوا الْكَيْلَ وَلَا تَكُونُوا مِنَ الْمُخْسِرِينَ"
},
{
    "id": "3114",
    "sura": "26",
    "VerseIDAr": "182",
    "ayat": "وَزِنُوا بِالْقِسْطَاسِ الْمُسْتَقِيمِ"
},
{
    "id": "3115",
    "sura": "26",
    "VerseIDAr": "183",
    "ayat": "وَلَا تَبْخَسُوا النَّاسَ أَشْيَاءَهُمْ وَلَا تَعْثَوْا فِي الْأَرْضِ مُفْسِدِينَ"
},
{
    "id": "3116",
    "sura": "26",
    "VerseIDAr": "184",
    "ayat": "وَاتَّقُوا الَّذِي خَلَقَكُمْ وَالْجِبِلَّةَ الْأَوَّلِينَ"
},
{
    "id": "3117",
    "sura": "26",
    "VerseIDAr": "185",
    "ayat": "قَالُوا إِنَّمَا أَنْتَ مِنَ الْمُسَحَّرِينَ"
},
{
    "id": "3118",
    "sura": "26",
    "VerseIDAr": "186",
    "ayat": "وَمَا أَنْتَ إِلَّا بَشَرٌ مِثْلُنَا وَإِنْ نَظُنُّكَ لَمِنَ الْكَاذِبِينَ"
},
{
    "id": "3119",
    "sura": "26",
    "VerseIDAr": "187",
    "ayat": "فَأَسْقِطْ عَلَيْنَا كِسَفًا مِنَ السَّمَاءِ إِنْ كُنْتَ مِنَ الصَّادِقِينَ"
},
{
    "id": "3120",
    "sura": "26",
    "VerseIDAr": "188",
    "ayat": "قَالَ رَبِّي أَعْلَمُ بِمَا تَعْمَلُونَ"
},
{
    "id": "3121",
    "sura": "26",
    "VerseIDAr": "189",
    "ayat": "فَكَذَّبُوهُ فَأَخَذَهُمْ عَذَابُ يَوْمِ الظُّلَّةِ ۚ إِنَّهُ كَانَ عَذَابَ يَوْمٍ عَظِيمٍ"
},
{
    "id": "3122",
    "sura": "26",
    "VerseIDAr": "190",
    "ayat": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُمْ مُؤْمِنِينَ"
},
{
    "id": "3123",
    "sura": "26",
    "VerseIDAr": "191",
    "ayat": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ"
},
{
    "id": "3124",
    "sura": "26",
    "VerseIDAr": "192",
    "ayat": "وَإِنَّهُ لَتَنْزِيلُ رَبِّ الْعَالَمِينَ"
},
{
    "id": "3125",
    "sura": "26",
    "VerseIDAr": "193",
    "ayat": "نَزَلَ بِهِ الرُّوحُ الْأَمِينُ"
},
{
    "id": "3126",
    "sura": "26",
    "VerseIDAr": "194",
    "ayat": "عَلَىٰ قَلْبِكَ لِتَكُونَ مِنَ الْمُنْذِرِينَ"
},
{
    "id": "3127",
    "sura": "26",
    "VerseIDAr": "195",
    "ayat": "بِلِسَانٍ عَرَبِيٍّ مُبِينٍ"
},
{
    "id": "3128",
    "sura": "26",
    "VerseIDAr": "196",
    "ayat": "وَإِنَّهُ لَفِي زُبُرِ الْأَوَّلِينَ"
},
{
    "id": "3129",
    "sura": "26",
    "VerseIDAr": "197",
    "ayat": "أَوَلَمْ يَكُنْ لَهُمْ آيَةً أَنْ يَعْلَمَهُ عُلَمَاءُ بَنِي إِسْرَائِيلَ"
},
{
    "id": "3130",
    "sura": "26",
    "VerseIDAr": "198",
    "ayat": "وَلَوْ نَزَّلْنَاهُ عَلَىٰ بَعْضِ الْأَعْجَمِينَ"
},
{
    "id": "3131",
    "sura": "26",
    "VerseIDAr": "199",
    "ayat": "فَقَرَأَهُ عَلَيْهِمْ مَا كَانُوا بِهِ مُؤْمِنِينَ"
},
{
    "id": "3132",
    "sura": "26",
    "VerseIDAr": "200",
    "ayat": "كَذَٰلِكَ سَلَكْنَاهُ فِي قُلُوبِ الْمُجْرِمِينَ"
},
{
    "id": "3133",
    "sura": "26",
    "VerseIDAr": "201",
    "ayat": "لَا يُؤْمِنُونَ بِهِ حَتَّىٰ يَرَوُا الْعَذَابَ الْأَلِيمَ"
},
{
    "id": "3134",
    "sura": "26",
    "VerseIDAr": "202",
    "ayat": "فَيَأْتِيَهُمْ بَغْتَةً وَهُمْ لَا يَشْعُرُونَ"
},
{
    "id": "3135",
    "sura": "26",
    "VerseIDAr": "203",
    "ayat": "فَيَقُولُوا هَلْ نَحْنُ مُنْظَرُونَ"
},
{
    "id": "3136",
    "sura": "26",
    "VerseIDAr": "204",
    "ayat": "أَفَبِعَذَابِنَا يَسْتَعْجِلُونَ"
},
{
    "id": "3137",
    "sura": "26",
    "VerseIDAr": "205",
    "ayat": "أَفَرَأَيْتَ إِنْ مَتَّعْنَاهُمْ سِنِينَ"
},
{
    "id": "3138",
    "sura": "26",
    "VerseIDAr": "206",
    "ayat": "ثُمَّ جَاءَهُمْ مَا كَانُوا يُوعَدُونَ"
},
{
    "id": "3139",
    "sura": "26",
    "VerseIDAr": "207",
    "ayat": "مَا أَغْنَىٰ عَنْهُمْ مَا كَانُوا يُمَتَّعُونَ"
},
{
    "id": "3140",
    "sura": "26",
    "VerseIDAr": "208",
    "ayat": "وَمَا أَهْلَكْنَا مِنْ قَرْيَةٍ إِلَّا لَهَا مُنْذِرُونَ"
},
{
    "id": "3141",
    "sura": "26",
    "VerseIDAr": "209",
    "ayat": "ذِكْرَىٰ وَمَا كُنَّا ظَالِمِينَ"
},
{
    "id": "3142",
    "sura": "26",
    "VerseIDAr": "210",
    "ayat": "وَمَا تَنَزَّلَتْ بِهِ الشَّيَاطِينُ"
},
{
    "id": "3143",
    "sura": "26",
    "VerseIDAr": "211",
    "ayat": "وَمَا يَنْبَغِي لَهُمْ وَمَا يَسْتَطِيعُونَ"
},
{
    "id": "3144",
    "sura": "26",
    "VerseIDAr": "212",
    "ayat": "إِنَّهُمْ عَنِ السَّمْعِ لَمَعْزُولُونَ"
},
{
    "id": "3145",
    "sura": "26",
    "VerseIDAr": "213",
    "ayat": "فَلَا تَدْعُ مَعَ اللَّهِ إِلَٰهًا آخَرَ فَتَكُونَ مِنَ الْمُعَذَّبِينَ"
},
{
    "id": "3146",
    "sura": "26",
    "VerseIDAr": "214",
    "ayat": "وَأَنْذِرْ عَشِيرَتَكَ الْأَقْرَبِينَ"
},
{
    "id": "3147",
    "sura": "26",
    "VerseIDAr": "215",
    "ayat": "وَاخْفِضْ جَنَاحَكَ لِمَنِ اتَّبَعَكَ مِنَ الْمُؤْمِنِينَ"
},
{
    "id": "3148",
    "sura": "26",
    "VerseIDAr": "216",
    "ayat": "فَإِنْ عَصَوْكَ فَقُلْ إِنِّي بَرِيءٌ مِمَّا تَعْمَلُونَ"
},
{
    "id": "3149",
    "sura": "26",
    "VerseIDAr": "217",
    "ayat": "وَتَوَكَّلْ عَلَى الْعَزِيزِ الرَّحِيمِ"
},
{
    "id": "3150",
    "sura": "26",
    "VerseIDAr": "218",
    "ayat": "الَّذِي يَرَاكَ حِينَ تَقُومُ"
},
{
    "id": "3151",
    "sura": "26",
    "VerseIDAr": "219",
    "ayat": "وَتَقَلُّبَكَ فِي السَّاجِدِينَ"
},
{
    "id": "3152",
    "sura": "26",
    "VerseIDAr": "220",
    "ayat": "إِنَّهُ هُوَ السَّمِيعُ الْعَلِيمُ"
},
{
    "id": "3153",
    "sura": "26",
    "VerseIDAr": "221",
    "ayat": "هَلْ أُنَبِّئُكُمْ عَلَىٰ مَنْ تَنَزَّلُ الشَّيَاطِينُ"
},
{
    "id": "3154",
    "sura": "26",
    "VerseIDAr": "222",
    "ayat": "تَنَزَّلُ عَلَىٰ كُلِّ أَفَّاكٍ أَثِيمٍ"
},
{
    "id": "3155",
    "sura": "26",
    "VerseIDAr": "223",
    "ayat": "يُلْقُونَ السَّمْعَ وَأَكْثَرُهُمْ كَاذِبُونَ"
},
{
    "id": "3156",
    "sura": "26",
    "VerseIDAr": "224",
    "ayat": "وَالشُّعَرَاءُ يَتَّبِعُهُمُ الْغَاوُونَ"
},
{
    "id": "3157",
    "sura": "26",
    "VerseIDAr": "225",
    "ayat": "أَلَمْ تَرَ أَنَّهُمْ فِي كُلِّ وَادٍ يَهِيمُونَ"
},
{
    "id": "3158",
    "sura": "26",
    "VerseIDAr": "226",
    "ayat": "وَأَنَّهُمْ يَقُولُونَ مَا لَا يَفْعَلُونَ"
},
{
    "id": "3159",
    "sura": "26",
    "VerseIDAr": "227",
    "ayat": "إِلَّا الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ وَذَكَرُوا اللَّهَ كَثِيرًا وَانْتَصَرُوا مِنْ بَعْدِ مَا ظُلِمُوا ۗ وَسَيَعْلَمُ الَّذِينَ ظَلَمُوا أَيَّ مُنْقَلَبٍ يَنْقَلِبُونَ"
}
]