[
{
    "id": "4980",
    "sura": "56",
    "VerseIDAr": "1",
    "ayat": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ إِذَا وَقَعَتِ الْوَاقِعَةُ"
},
{
    "id": "4981",
    "sura": "56",
    "VerseIDAr": "2",
    "ayat": "لَيْسَ لِوَقْعَتِهَا كَاذِبَةٌ"
},
{
    "id": "4982",
    "sura": "56",
    "VerseIDAr": "3",
    "ayat": "خَافِضَةٌ رَافِعَةٌ"
},
{
    "id": "4983",
    "sura": "56",
    "VerseIDAr": "4",
    "ayat": "إِذَا رُجَّتِ الْأَرْضُ رَجًّا"
},
{
    "id": "4984",
    "sura": "56",
    "VerseIDAr": "5",
    "ayat": "وَبُسَّتِ الْجِبَالُ بَسًّا"
},
{
    "id": "4985",
    "sura": "56",
    "VerseIDAr": "6",
    "ayat": "فَكَانَتْ هَبَاءً مُنْبَثًّا"
},
{
    "id": "4986",
    "sura": "56",
    "VerseIDAr": "7",
    "ayat": "وَكُنْتُمْ أَزْوَاجًا ثَلَاثَةً"
},
{
    "id": "4987",
    "sura": "56",
    "VerseIDAr": "8",
    "ayat": "فَأَصْحَابُ الْمَيْمَنَةِ مَا أَصْحَابُ الْمَيْمَنَةِ"
},
{
    "id": "4988",
    "sura": "56",
    "VerseIDAr": "9",
    "ayat": "وَأَصْحَابُ الْمَشْأَمَةِ مَا أَصْحَابُ الْمَشْأَمَةِ"
},
{
    "id": "4989",
    "sura": "56",
    "VerseIDAr": "10",
    "ayat": "وَالسَّابِقُونَ السَّابِقُونَ"
},
{
    "id": "4990",
    "sura": "56",
    "VerseIDAr": "11",
    "ayat": "أُولَٰئِكَ الْمُقَرَّبُونَ"
},
{
    "id": "4991",
    "sura": "56",
    "VerseIDAr": "12",
    "ayat": "فِي جَنَّاتِ النَّعِيمِ"
},
{
    "id": "4992",
    "sura": "56",
    "VerseIDAr": "13",
    "ayat": "ثُلَّةٌ مِنَ الْأَوَّلِينَ"
},
{
    "id": "4993",
    "sura": "56",
    "VerseIDAr": "14",
    "ayat": "وَقَلِيلٌ مِنَ الْآخِرِينَ"
},
{
    "id": "4994",
    "sura": "56",
    "VerseIDAr": "15",
    "ayat": "عَلَىٰ سُرُرٍ مَوْضُونَةٍ"
},
{
    "id": "4995",
    "sura": "56",
    "VerseIDAr": "16",
    "ayat": "مُتَّكِئِينَ عَلَيْهَا مُتَقَابِلِينَ"
},
{
    "id": "4996",
    "sura": "56",
    "VerseIDAr": "17",
    "ayat": "يَطُوفُ عَلَيْهِمْ وِلْدَانٌ مُخَلَّدُونَ"
},
{
    "id": "4997",
    "sura": "56",
    "VerseIDAr": "18",
    "ayat": "بِأَكْوَابٍ وَأَبَارِيقَ وَكَأْسٍ مِنْ مَعِينٍ"
},
{
    "id": "4998",
    "sura": "56",
    "VerseIDAr": "19",
    "ayat": "لَا يُصَدَّعُونَ عَنْهَا وَلَا يُنْزِفُونَ"
},
{
    "id": "4999",
    "sura": "56",
    "VerseIDAr": "20",
    "ayat": "وَفَاكِهَةٍ مِمَّا يَتَخَيَّرُونَ"
},
{
    "id": "5000",
    "sura": "56",
    "VerseIDAr": "21",
    "ayat": "وَلَحْمِ طَيْرٍ مِمَّا يَشْتَهُونَ"
},
{
    "id": "5001",
    "sura": "56",
    "VerseIDAr": "22",
    "ayat": "وَحُورٌ عِينٌ"
},
{
    "id": "5002",
    "sura": "56",
    "VerseIDAr": "23",
    "ayat": "كَأَمْثَالِ اللُّؤْلُؤِ الْمَكْنُونِ"
},
{
    "id": "5003",
    "sura": "56",
    "VerseIDAr": "24",
    "ayat": "جَزَاءً بِمَا كَانُوا يَعْمَلُونَ"
},
{
    "id": "5004",
    "sura": "56",
    "VerseIDAr": "25",
    "ayat": "لَا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا تَأْثِيمًا"
},
{
    "id": "5005",
    "sura": "56",
    "VerseIDAr": "26",
    "ayat": "إِلَّا قِيلًا سَلَامًا سَلَامًا"
},
{
    "id": "5006",
    "sura": "56",
    "VerseIDAr": "27",
    "ayat": "وَأَصْحَابُ الْيَمِينِ مَا أَصْحَابُ الْيَمِينِ"
},
{
    "id": "5007",
    "sura": "56",
    "VerseIDAr": "28",
    "ayat": "فِي سِدْرٍ مَخْضُودٍ"
},
{
    "id": "5008",
    "sura": "56",
    "VerseIDAr": "29",
    "ayat": "وَطَلْحٍ مَنْضُودٍ"
},
{
    "id": "5009",
    "sura": "56",
    "VerseIDAr": "30",
    "ayat": "وَظِلٍّ مَمْدُودٍ"
},
{
    "id": "5010",
    "sura": "56",
    "VerseIDAr": "31",
    "ayat": "وَمَاءٍ مَسْكُوبٍ"
},
{
    "id": "5011",
    "sura": "56",
    "VerseIDAr": "32",
    "ayat": "وَفَاكِهَةٍ كَثِيرَةٍ"
},
{
    "id": "5012",
    "sura": "56",
    "VerseIDAr": "33",
    "ayat": "لَا مَقْطُوعَةٍ وَلَا مَمْنُوعَةٍ"
},
{
    "id": "5013",
    "sura": "56",
    "VerseIDAr": "34",
    "ayat": "وَفُرُشٍ مَرْفُوعَةٍ"
},
{
    "id": "5014",
    "sura": "56",
    "VerseIDAr": "35",
    "ayat": "إِنَّا أَنْشَأْنَاهُنَّ إِنْشَاءً"
},
{
    "id": "5015",
    "sura": "56",
    "VerseIDAr": "36",
    "ayat": "فَجَعَلْنَاهُنَّ أَبْكَارًا"
},
{
    "id": "5016",
    "sura": "56",
    "VerseIDAr": "37",
    "ayat": "عُرُبًا أَتْرَابًا"
},
{
    "id": "5017",
    "sura": "56",
    "VerseIDAr": "38",
    "ayat": "لِأَصْحَابِ الْيَمِينِ"
},
{
    "id": "5018",
    "sura": "56",
    "VerseIDAr": "39",
    "ayat": "ثُلَّةٌ مِنَ الْأَوَّلِينَ"
},
{
    "id": "5019",
    "sura": "56",
    "VerseIDAr": "40",
    "ayat": "وَثُلَّةٌ مِنَ الْآخِرِينَ"
},
{
    "id": "5020",
    "sura": "56",
    "VerseIDAr": "41",
    "ayat": "وَأَصْحَابُ الشِّمَالِ مَا أَصْحَابُ الشِّمَالِ"
},
{
    "id": "5021",
    "sura": "56",
    "VerseIDAr": "42",
    "ayat": "فِي سَمُومٍ وَحَمِيمٍ"
},
{
    "id": "5022",
    "sura": "56",
    "VerseIDAr": "43",
    "ayat": "وَظِلٍّ مِنْ يَحْمُومٍ"
},
{
    "id": "5023",
    "sura": "56",
    "VerseIDAr": "44",
    "ayat": "لَا بَارِدٍ وَلَا كَرِيمٍ"
},
{
    "id": "5024",
    "sura": "56",
    "VerseIDAr": "45",
    "ayat": "إِنَّهُمْ كَانُوا قَبْلَ ذَٰلِكَ مُتْرَفِينَ"
},
{
    "id": "5025",
    "sura": "56",
    "VerseIDAr": "46",
    "ayat": "وَكَانُوا يُصِرُّونَ عَلَى الْحِنْثِ الْعَظِيمِ"
},
{
    "id": "5026",
    "sura": "56",
    "VerseIDAr": "47",
    "ayat": "وَكَانُوا يَقُولُونَ أَئِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَامًا أَإِنَّا لَمَبْعُوثُونَ"
},
{
    "id": "5027",
    "sura": "56",
    "VerseIDAr": "48",
    "ayat": "أَوَآبَاؤُنَا الْأَوَّلُونَ"
},
{
    "id": "5028",
    "sura": "56",
    "VerseIDAr": "49",
    "ayat": "قُلْ إِنَّ الْأَوَّلِينَ وَالْآخِرِينَ"
},
{
    "id": "5029",
    "sura": "56",
    "VerseIDAr": "50",
    "ayat": "لَمَجْمُوعُونَ إِلَىٰ مِيقَاتِ يَوْمٍ مَعْلُومٍ"
},
{
    "id": "5030",
    "sura": "56",
    "VerseIDAr": "51",
    "ayat": "ثُمَّ إِنَّكُمْ أَيُّهَا الضَّالُّونَ الْمُكَذِّبُونَ"
},
{
    "id": "5031",
    "sura": "56",
    "VerseIDAr": "52",
    "ayat": "لَآكِلُونَ مِنْ شَجَرٍ مِنْ زَقُّومٍ"
},
{
    "id": "5032",
    "sura": "56",
    "VerseIDAr": "53",
    "ayat": "فَمَالِئُونَ مِنْهَا الْبُطُونَ"
},
{
    "id": "5033",
    "sura": "56",
    "VerseIDAr": "54",
    "ayat": "فَشَارِبُونَ عَلَيْهِ مِنَ الْحَمِيمِ"
},
{
    "id": "5034",
    "sura": "56",
    "VerseIDAr": "55",
    "ayat": "فَشَارِبُونَ شُرْبَ الْهِيمِ"
},
{
    "id": "5035",
    "sura": "56",
    "VerseIDAr": "56",
    "ayat": "هَٰذَا نُزُلُهُمْ يَوْمَ الدِّينِ"
},
{
    "id": "5036",
    "sura": "56",
    "VerseIDAr": "57",
    "ayat": "نَحْنُ خَلَقْنَاكُمْ فَلَوْلَا تُصَدِّقُونَ"
},
{
    "id": "5037",
    "sura": "56",
    "VerseIDAr": "58",
    "ayat": "أَفَرَأَيْتُمْ مَا تُمْنُونَ"
},
{
    "id": "5038",
    "sura": "56",
    "VerseIDAr": "59",
    "ayat": "أَأَنْتُمْ تَخْلُقُونَهُ أَمْ نَحْنُ الْخَالِقُونَ"
},
{
    "id": "5039",
    "sura": "56",
    "VerseIDAr": "60",
    "ayat": "نَحْنُ قَدَّرْنَا بَيْنَكُمُ الْمَوْتَ وَمَا نَحْنُ بِمَسْبُوقِينَ"
},
{
    "id": "5040",
    "sura": "56",
    "VerseIDAr": "61",
    "ayat": "عَلَىٰ أَنْ نُبَدِّلَ أَمْثَالَكُمْ وَنُنْشِئَكُمْ فِي مَا لَا تَعْلَمُونَ"
},
{
    "id": "5041",
    "sura": "56",
    "VerseIDAr": "62",
    "ayat": "وَلَقَدْ عَلِمْتُمُ النَّشْأَةَ الْأُولَىٰ فَلَوْلَا تَذَكَّرُونَ"
},
{
    "id": "5042",
    "sura": "56",
    "VerseIDAr": "63",
    "ayat": "أَفَرَأَيْتُمْ مَا تَحْرُثُونَ"
},
{
    "id": "5043",
    "sura": "56",
    "VerseIDAr": "64",
    "ayat": "أَأَنْتُمْ تَزْرَعُونَهُ أَمْ نَحْنُ الزَّارِعُونَ"
},
{
    "id": "5044",
    "sura": "56",
    "VerseIDAr": "65",
    "ayat": "لَوْ نَشَاءُ لَجَعَلْنَاهُ حُطَامًا فَظَلْتُمْ تَفَكَّهُونَ"
},
{
    "id": "5045",
    "sura": "56",
    "VerseIDAr": "66",
    "ayat": "إِنَّا لَمُغْرَمُونَ"
},
{
    "id": "5046",
    "sura": "56",
    "VerseIDAr": "67",
    "ayat": "بَلْ نَحْنُ مَحْرُومُونَ"
},
{
    "id": "5047",
    "sura": "56",
    "VerseIDAr": "68",
    "ayat": "أَفَرَأَيْتُمُ الْمَاءَ الَّذِي تَشْرَبُونَ"
},
{
    "id": "5048",
    "sura": "56",
    "VerseIDAr": "69",
    "ayat": "أَأَنْتُمْ أَنْزَلْتُمُوهُ مِنَ الْمُزْنِ أَمْ نَحْنُ الْمُنْزِلُونَ"
},
{
    "id": "5049",
    "sura": "56",
    "VerseIDAr": "70",
    "ayat": "لَوْ نَشَاءُ جَعَلْنَاهُ أُجَاجًا فَلَوْلَا تَشْكُرُونَ"
},
{
    "id": "5050",
    "sura": "56",
    "VerseIDAr": "71",
    "ayat": "أَفَرَأَيْتُمُ النَّارَ الَّتِي تُورُونَ"
},
{
    "id": "5051",
    "sura": "56",
    "VerseIDAr": "72",
    "ayat": "أَأَنْتُمْ أَنْشَأْتُمْ شَجَرَتَهَا أَمْ نَحْنُ الْمُنْشِئُونَ"
},
{
    "id": "5052",
    "sura": "56",
    "VerseIDAr": "73",
    "ayat": "نَحْنُ جَعَلْنَاهَا تَذْكِرَةً وَمَتَاعًا لِلْمُقْوِينَ"
},
{
    "id": "5053",
    "sura": "56",
    "VerseIDAr": "74",
    "ayat": "فَسَبِّحْ بِاسْمِ رَبِّكَ الْعَظِيمِ"
},
{
    "id": "5054",
    "sura": "56",
    "VerseIDAr": "75",
    "ayat": "فَلَا أُقْسِمُ بِمَوَاقِعِ النُّجُومِ"
},
{
    "id": "5055",
    "sura": "56",
    "VerseIDAr": "76",
    "ayat": "وَإِنَّهُ لَقَسَمٌ لَوْ تَعْلَمُونَ عَظِيمٌ"
},
{
    "id": "5056",
    "sura": "56",
    "VerseIDAr": "77",
    "ayat": "إِنَّهُ لَقُرْآنٌ كَرِيمٌ"
},
{
    "id": "5057",
    "sura": "56",
    "VerseIDAr": "78",
    "ayat": "فِي كِتَابٍ مَكْنُونٍ"
},
{
    "id": "5058",
    "sura": "56",
    "VerseIDAr": "79",
    "ayat": "لَا يَمَسُّهُ إِلَّا الْمُطَهَّرُونَ"
},
{
    "id": "5059",
    "sura": "56",
    "VerseIDAr": "80",
    "ayat": "تَنْزِيلٌ مِنْ رَبِّ الْعَالَمِينَ"
},
{
    "id": "5060",
    "sura": "56",
    "VerseIDAr": "81",
    "ayat": "أَفَبِهَٰذَا الْحَدِيثِ أَنْتُمْ مُدْهِنُونَ"
},
{
    "id": "5061",
    "sura": "56",
    "VerseIDAr": "82",
    "ayat": "وَتَجْعَلُونَ رِزْقَكُمْ أَنَّكُمْ تُكَذِّبُونَ"
},
{
    "id": "5062",
    "sura": "56",
    "VerseIDAr": "83",
    "ayat": "فَلَوْلَا إِذَا بَلَغَتِ الْحُلْقُومَ"
},
{
    "id": "5063",
    "sura": "56",
    "VerseIDAr": "84",
    "ayat": "وَأَنْتُمْ حِينَئِذٍ تَنْظُرُونَ"
},
{
    "id": "5064",
    "sura": "56",
    "VerseIDAr": "85",
    "ayat": "وَنَحْنُ أَقْرَبُ إِلَيْهِ مِنْكُمْ وَلَٰكِنْ لَا تُبْصِرُونَ"
},
{
    "id": "5065",
    "sura": "56",
    "VerseIDAr": "86",
    "ayat": "فَلَوْلَا إِنْ كُنْتُمْ غَيْرَ مَدِينِينَ"
},
{
    "id": "5066",
    "sura": "56",
    "VerseIDAr": "87",
    "ayat": "تَرْجِعُونَهَا إِنْ كُنْتُمْ صَادِقِينَ"
},
{
    "id": "5067",
    "sura": "56",
    "VerseIDAr": "88",
    "ayat": "فَأَمَّا إِنْ كَانَ مِنَ الْمُقَرَّبِينَ"
},
{
    "id": "5068",
    "sura": "56",
    "VerseIDAr": "89",
    "ayat": "فَرَوْحٌ وَرَيْحَانٌ وَجَنَّتُ نَعِيمٍ"
},
{
    "id": "5069",
    "sura": "56",
    "VerseIDAr": "90",
    "ayat": "وَأَمَّا إِنْ كَانَ مِنْ أَصْحَابِ الْيَمِينِ"
},
{
    "id": "5070",
    "sura": "56",
    "VerseIDAr": "91",
    "ayat": "فَسَلَامٌ لَكَ مِنْ أَصْحَابِ الْيَمِينِ"
},
{
    "id": "5071",
    "sura": "56",
    "VerseIDAr": "92",
    "ayat": "وَأَمَّا إِنْ كَانَ مِنَ الْمُكَذِّبِينَ الضَّالِّينَ"
},
{
    "id": "5072",
    "sura": "56",
    "VerseIDAr": "93",
    "ayat": "فَنُزُلٌ مِنْ حَمِيمٍ"
