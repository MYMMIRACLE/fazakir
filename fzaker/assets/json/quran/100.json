[
{
    "id": "6147",
    "sura": "100",
    "VerseIDAr": "1",
    "ayat": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَالْعَادِيَاتِ ضَبْحًا"
},
{
    "id": "6148",
    "sura": "100",
    "VerseIDAr": "2",
    "ayat": "فَالْمُورِيَاتِ قَدْحًا"
},
{
    "id": "6149",
    "sura": "100",
    "VerseIDAr": "3",
    "ayat": "فَالْمُغِيرَاتِ صُبْحًا"
},
{
    "id": "6150",
    "sura": "100",
    "VerseIDAr": "4",
    "ayat": "فَأَثَرْنَ بِهِ نَقْعًا"
},
{
    "id": "6151",
    "sura": "100",
    "VerseIDAr": "5",
    "ayat": "فَوَسَطْنَ بِهِ جَمْعًا"
},
{
    "id": "6152",
    "sura": "100",
    "VerseIDAr": "6",
    "ayat": "إِنَّ الْإِنْسَانَ لِرَبِّهِ لَكَنُودٌ"
},
{
    "id": "6153",
    "sura": "100",
    "VerseIDAr": "7",
    "ayat": "وَإِنَّهُ عَلَىٰ ذَٰلِكَ لَشَهِيدٌ"
},
{
    "id": "6154",
    "sura": "100",
    "VerseIDAr": "8",
    "ayat": "وَإِنَّهُ لِحُبِّ الْخَيْرِ لَشَدِيدٌ"
},
{
    "id": "6155",
    "sura": "100",
    "VerseIDAr": "9",
    "ayat": "أَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِي الْقُبُورِ"
},
{
    "id": "6156",
    "sura": "100",
    "VerseIDAr": "10",
    "ayat": "وَحُصِّلَ مَا فِي الصُّدُورِ"
},
{
    "id": "6157",
    "sura": "100",
    "VerseIDAr": "11",
    "ayat": "إِنَّ رَبَّهُمْ بِهِمْ يَوْمَئِذٍ لَخَبِيرٌ"
}
