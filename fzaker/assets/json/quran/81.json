[
{
    "id": "5801",
    "sura": "81",
    "VerseIDAr": "1",
    "ayat": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ إِذَا الشَّمْسُ كُوِّرَتْ"
},
{
    "id": "5802",
    "sura": "81",
    "VerseIDAr": "2",
    "ayat": "وَإِذَا النُّجُومُ انْكَدَرَتْ"
},
{
    "id": "5803",
    "sura": "81",
    "VerseIDAr": "3",
    "ayat": "وَإِذَا الْجِبَالُ سُيِّرَتْ"
},
{
    "id": "5804",
    "sura": "81",
    "VerseIDAr": "4",
    "ayat": "وَإِذَا الْعِشَارُ عُطِّلَتْ"
},
{
    "id": "5805",
    "sura": "81",
    "VerseIDAr": "5",
    "ayat": "وَإِذَا الْوُحُوشُ حُشِرَتْ"
},
{
    "id": "5806",
    "sura": "81",
    "VerseIDAr": "6",
    "ayat": "وَإِذَا الْبِحَارُ سُجِّرَتْ"
},
{
    "id": "5807",
    "sura": "81",
    "VerseIDAr": "7",
    "ayat": "وَإِذَا النُّفُوسُ زُوِّجَتْ"
},
{
    "id": "5808",
    "sura": "81",
    "VerseIDAr": "8",
    "ayat": "وَإِذَا الْمَوْءُودَةُ سُئِلَتْ"
},
{
    "id": "5809",
    "sura": "81",
    "VerseIDAr": "9",
    "ayat": "بِأَيِّ ذَنْبٍ قُتِلَتْ"
},
{
    "id": "5810",
    "sura": "81",
    "VerseIDAr": "10",
    "ayat": "وَإِذَا الصُّحُفُ نُشِرَتْ"
},
{
    "id": "5811",
    "sura": "81",
    "VerseIDAr": "11",
    "ayat": "وَإِذَا السَّمَاءُ كُشِطَتْ"
},
{
    "id": "5812",
    "sura": "81",
    "VerseIDAr": "12",
    "ayat": "وَإِذَا الْجَحِيمُ سُعِّرَتْ"
},
{
    "id": "5813",
    "sura": "81",
    "VerseIDAr": "13",
    "ayat": "وَإِذَا الْجَنَّةُ أُزْلِفَتْ"
},
{
    "id": "5814",
    "sura": "81",
    "VerseIDAr": "14",
    "ayat": "عَلِمَتْ نَفْسٌ مَا أَحْضَرَتْ"
},
{
    "id": "5815",
    "sura": "81",
    "VerseIDAr": "15",
    "ayat": "فَلَا أُقْسِمُ بِالْخُنَّسِ"
},
{
    "id": "5816",
    "sura": "81",
    "VerseIDAr": "16",
    "ayat": "الْجَوَارِ الْكُنَّسِ"
},
{
    "id": "5817",
    "sura": "81",
    "VerseIDAr": "17",
    "ayat": "وَاللَّيْلِ إِذَا عَسْعَسَ"
},
{
    "id": "5818",
    "sura": "81",
    "VerseIDAr": "18",
    "ayat": "وَالصُّبْحِ إِذَا تَنَفَّسَ"
},
{
    "id": "5819",
    "sura": "81",
    "VerseIDAr": "19",
    "ayat": "إِنَّهُ لَقَوْلُ رَسُولٍ كَرِيمٍ"
},
{
    "id": "5820",
    "sura": "81",
    "VerseIDAr": "20",
    "ayat": "ذِي قُوَّةٍ عِنْدَ ذِي الْعَرْشِ مَكِينٍ"
},
{
    "id": "5821",
    "sura": "81",
    "VerseIDAr": "21",
    "ayat": "مُطَاعٍ ثَمَّ أَمِينٍ"
},
{
    "id": "5822",
    "sura": "81",
    "VerseIDAr": "22",
    "ayat": "وَمَا صَاحِبُكُمْ بِمَجْنُونٍ"
},
{
    "id": "5823",
    "sura": "81",
    "VerseIDAr": "23",
    "ayat": "وَلَقَدْ رَآهُ بِالْأُفُقِ الْمُبِينِ"
},
{
    "id": "5824",
    "sura": "81",
    "VerseIDAr": "24",
    "ayat": "وَمَا هُوَ عَلَى الْغَيْبِ بِضَنِينٍ"
},
{
    "id": "5825",
    "sura": "81",
    "VerseIDAr": "25",
    "ayat": "وَمَا هُوَ بِقَوْلِ شَيْطَانٍ رَجِيمٍ"
},
{
    "id": "5826",
    "sura": "81",
    "VerseIDAr": "26",
    "ayat": "فَأَيْنَ تَذْهَبُونَ"
},
{
    "id": "5827",
    "sura": "81",
    "VerseIDAr": "27",
    "ayat": "إِنْ هُوَ إِلَّا ذِكْرٌ لِلْعَالَمِينَ"
},
{
    "id": "5828",
    "sura": "81",
    "VerseIDAr": "28",
    "ayat": "لِمَنْ شَاءَ مِنْكُمْ أَنْ يَسْتَقِيمَ"
},
{
    "id": "5829",
    "sura": "81",
    "VerseIDAr": "29",
    "ayat": "وَمَا تَشَاءُونَ إِلَّا أَنْ يَشَاءَ اللَّهُ رَبُّ الْعَالَمِينَ"
}
]