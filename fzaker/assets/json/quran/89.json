[
{
    "id": "5994",
    "sura": "89",
    "VerseIDAr": "1",
    "ayat": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَالْفَجْرِ"
},
{
    "id": "5995",
    "sura": "89",
    "VerseIDAr": "2",
    "ayat": "وَلَيَالٍ عَشْرٍ"
},
{
    "id": "5996",
    "sura": "89",
    "VerseIDAr": "3",
    "ayat": "وَالشَّفْعِ وَالْوَتْرِ"
},
{
    "id": "5997",
    "sura": "89",
    "VerseIDAr": "4",
    "ayat": "وَاللَّيْلِ إِذَا يَسْرِ"
},
{
    "id": "5998",
    "sura": "89",
    "VerseIDAr": "5",
    "ayat": "هَلْ فِي ذَٰلِكَ قَسَمٌ لِذِي حِجْرٍ"
},
{
    "id": "5999",
    "sura": "89",
    "VerseIDAr": "6",
    "ayat": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ"
},
{
    "id": "6000",
    "sura": "89",
    "VerseIDAr": "7",
    "ayat": "إِرَمَ ذَاتِ الْعِمَادِ"
},
{
    "id": "6001",
    "sura": "89",
    "VerseIDAr": "8",
    "ayat": "الَّتِي لَمْ يُخْلَقْ مِثْلُهَا فِي الْبِلَادِ"
},
{
    "id": "6002",
    "sura": "89",
    "VerseIDAr": "9",
    "ayat": "وَثَمُودَ الَّذِينَ جَابُوا الصَّخْرَ بِالْوَادِ"
},
{
    "id": "6003",
    "sura": "89",
    "VerseIDAr": "10",
    "ayat": "وَفِرْعَوْنَ ذِي الْأَوْتَادِ"
},
{
    "id": "6004",
    "sura": "89",
    "VerseIDAr": "11",
    "ayat": "الَّذِينَ طَغَوْا فِي الْبِلَادِ"
},
{
    "id": "6005",
    "sura": "89",
    "VerseIDAr": "12",
    "ayat": "فَأَكْثَرُوا فِيهَا الْفَسَادَ"
},
{
    "id": "6006",
    "sura": "89",
    "VerseIDAr": "13",
    "ayat": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ"
},
{
    "id": "6007",
    "sura": "89",
    "VerseIDAr": "14",
    "ayat": "إِنَّ رَبَّكَ لَبِالْمِرْصَادِ"
},
{
    "id": "6008",
    "sura": "89",
    "VerseIDAr": "15",
    "ayat": "فَأَمَّا الْإِنْسَانُ إِذَا مَا ابْتَلَاهُ رَبُّهُ فَأَكْرَمَهُ وَنَعَّمَهُ فَيَقُولُ رَبِّي أَكْرَمَنِ"
},
{
    "id": "6009",
    "sura": "89",
    "VerseIDAr": "16",
    "ayat": "وَأَمَّا إِذَا مَا ابْتَلَاهُ فَقَدَرَ عَلَيْهِ رِزْقَهُ فَيَقُولُ رَبِّي أَهَانَنِ"
},
{
    "id": "6010",
    "sura": "89",
    "VerseIDAr": "17",
    "ayat": "كَلَّا ۖ بَلْ لَا تُكْرِمُونَ الْيَتِيمَ"
},
{
    "id": "6011",
    "sura": "89",
    "VerseIDAr": "18",
    "ayat": "وَلَا تَحَاضُّونَ عَلَىٰ طَعَامِ الْمِسْكِينِ"
},
{
    "id": "6012",
    "sura": "89",
    "VerseIDAr": "19",
    "ayat": "وَتَأْكُلُونَ التُّرَاثَ أَكْلًا لَمًّا"
},
{
    "id": "6013",
    "sura": "89",
    "VerseIDAr": "20",
    "ayat": "وَتُحِبُّونَ الْمَالَ حُبًّا جَمًّا"
},
{
    "id": "6014",
    "sura": "89",
    "VerseIDAr": "21",
    "ayat": "كَلَّا إِذَا دُكَّتِ الْأَرْضُ دَكًّا دَكًّا"
},
{
    "id": "6015",
    "sura": "89",
    "VerseIDAr": "22",
    "ayat": "وَجَاءَ رَبُّكَ وَالْمَلَكُ صَفًّا صَفًّا"
},
{
    "id": "6016",
    "sura": "89",
    "VerseIDAr": "23",
    "ayat": "وَجِيءَ يَوْمَئِذٍ بِجَهَنَّمَ ۚ يَوْمَئِذٍ يَتَذَكَّرُ الْإِنْسَانُ وَأَنَّىٰ لَهُ الذِّكْرَىٰ"
},
{
    "id": "6017",
    "sura": "89",
    "VerseIDAr": "24",
    "ayat": "يَقُولُ يَا لَيْتَنِي قَدَّمْتُ لِحَيَاتِي"
},
{
    "id": "6018",
    "sura": "89",
    "VerseIDAr": "25",
    "ayat": "فَيَوْمَئِذٍ لَا يُعَذِّبُ عَذَابَهُ أَحَدٌ"
},
{
    "id": "6019",
    "sura": "89",
    "VerseIDAr": "26",
    "ayat": "وَلَا يُوثِقُ وَثَاقَهُ أَحَدٌ"
},
{
    "id": "6020",
    "sura": "89",
    "VerseIDAr": "27",
    "ayat": "يَا أَيَّتُهَا النَّفْسُ الْمُطْمَئِنَّةُ"
},
{
    "id": "6021",
    "sura": "89",
    "VerseIDAr": "28",
    "ayat": "ارْجِعِي إِلَىٰ رَبِّكِ رَاضِيَةً مَرْضِيَّةً"
},
{
    "id": "6022",
    "sura": "89",
    "VerseIDAr": "29",
    "ayat": "فَادْخُلِي فِي عِبَادِي"
},
{
    "id": "6023",
    "sura": "89",
    "VerseIDAr": "30",
    "ayat": "وَادْخُلِي جَنَّتِي"
}
]