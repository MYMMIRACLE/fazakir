    "id": "3960",
    "sura": "37",
    "VerseIDAr": "172",
    "ayat": "إِنَّهُمْ لَهُمُ الْمَنْصُورُونَ"
},
{
    "id": "3961",
    "sura": "37",
    "VerseIDAr": "173",
    "ayat": "وَإِنَّ جُنْدَنَا لَهُمُ الْغَالِبُونَ"
},
{
    "id": "3962",
    "sura": "37",
    "VerseIDAr": "174",
    "ayat": "فَتَوَلَّ عَنْهُمْ حَتَّىٰ حِينٍ"
},
{
    "id": "3963",
    "sura": "37",
    "VerseIDAr": "175",
    "ayat": "وَأَبْصِرْهُمْ فَسَوْفَ يُبْصِرُونَ"
},
{
    "id": "3964",
    "sura": "37",
    "VerseIDAr": "176",
    "ayat": "أَفَبِعَذَابِنَا يَسْتَعْجِلُونَ"
},
{
    "id": "3965",
    "sura": "37",
    "VerseIDAr": "177",
    "ayat": "فَإِذَا نَزَلَ بِسَاحَتِهِمْ فَسَاءَ صَبَاحُ الْمُنْذَرِينَ"
},
{
    "id": "3966",
    "sura": "37",
    "VerseIDAr": "178",
    "ayat": "وَتَوَلَّ عَنْهُمْ حَتَّىٰ حِينٍ"
},
{
    "id": "3967",
    "sura": "37",
    "VerseIDAr": "179",
    "ayat": "وَأَبْصِرْ فَسَوْفَ يُبْصِرُونَ"
},
{
    "id": "3968",
    "sura": "37",
    "VerseIDAr": "180",
    "ayat": "سُبْحَانَ رَبِّكَ رَبِّ الْعِزَّةِ عَمَّا يَصِفُونَ"
},
{
    "id": "3969",
    "sura": "37",
    "VerseIDAr": "181",
    "ayat": "وَسَلَامٌ عَلَى الْمُرْسَلِينَ"
},
{
    "id": "3970",
    "sura": "37",
    "VerseIDAr": "182",
    "ayat": "وَالْحَمْدُ لِلَّهِ رَبِّ الْعَالَمِينَ"
}
]