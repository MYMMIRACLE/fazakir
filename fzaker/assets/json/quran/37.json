[
{
    "id": "3789",
    "sura": "37",
    "VerseIDAr": "1",
    "ayat": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَالصَّافَّاتِ صَفًّا"
},
{
    "id": "3790",
    "sura": "37",
    "VerseIDAr": "2",
    "ayat": "فَالزَّاجِرَاتِ زَجْرًا"
},
{
    "id": "3791",
    "sura": "37",
    "VerseIDAr": "3",
    "ayat": "فَالتَّالِيَاتِ ذِكْرًا"
},
{
    "id": "3792",
    "sura": "37",
    "VerseIDAr": "4",
    "ayat": "إِنَّ إِلَٰهَكُمْ لَوَاحِدٌ"
},
{
    "id": "3793",
    "sura": "37",
    "VerseIDAr": "5",
    "ayat": "رَبُّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا وَرَبُّ الْمَشَارِقِ"
},
{
    "id": "3794",
    "sura": "37",
    "VerseIDAr": "6",
    "ayat": "إِنَّا زَيَّنَّا السَّمَاءَ الدُّنْيَا بِزِينَةٍ الْكَوَاكِبِ"
},
{
    "id": "3795",
    "sura": "37",
    "VerseIDAr": "7",
    "ayat": "وَحِفْظًا مِنْ كُلِّ شَيْطَانٍ مَارِدٍ"
},
{
    "id": "3796",
    "sura": "37",
    "VerseIDAr": "8",
    "ayat": "لَا يَسَّمَّعُونَ إِلَى الْمَلَإِ الْأَعْلَىٰ وَيُقْذَفُونَ مِنْ كُلِّ جَانِبٍ"
},
{
    "id": "3797",
    "sura": "37",
    "VerseIDAr": "9",
    "ayat": "دُحُورًا ۖ وَلَهُمْ عَذَابٌ وَاصِبٌ"
},
{
    "id": "3798",
    "sura": "37",
    "VerseIDAr": "10",
    "ayat": "إِلَّا مَنْ خَطِفَ الْخَطْفَةَ فَأَتْبَعَهُ شِهَابٌ ثَاقِبٌ"
},
{
    "id": "3799",
    "sura": "37",
    "VerseIDAr": "11",
    "ayat": "فَاسْتَفْتِهِمْ أَهُمْ أَشَدُّ خَلْقًا أَمْ مَنْ خَلَقْنَا ۚ إِنَّا خَلَقْنَاهُمْ مِنْ طِينٍ لَازِبٍ"
},
{
    "id": "3800",
    "sura": "37",
    "VerseIDAr": "12",
    "ayat": "بَلْ عَجِبْتَ وَيَسْخَرُونَ"
},
{
    "id": "3801",
    "sura": "37",
    "VerseIDAr": "13",
    "ayat": "وَإِذَا ذُكِّرُوا لَا يَذْكُرُونَ"
},
{
    "id": "3802",
    "sura": "37",
    "VerseIDAr": "14",
    "ayat": "وَإِذَا رَأَوْا آيَةً يَسْتَسْخِرُونَ"
},
{
    "id": "3803",
    "sura": "37",
    "VerseIDAr": "15",
    "ayat": "وَقَالُوا إِنْ هَٰذَا إِلَّا سِحْرٌ مُبِينٌ"
},
{
    "id": "3804",
    "sura": "37",
    "VerseIDAr": "16",
    "ayat": "أَإِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَامًا أَإِنَّا لَمَبْعُوثُونَ"
},
{
    "id": "3805",
    "sura": "37",
    "VerseIDAr": "17",
    "ayat": "أَوَآبَاؤُنَا الْأَوَّلُونَ"
},
{
    "id": "3806",
    "sura": "37",
    "VerseIDAr": "18",
    "ayat": "قُلْ نَعَمْ وَأَنْتُمْ دَاخِرُونَ"
},
{
    "id": "3807",
    "sura": "37",
    "VerseIDAr": "19",
    "ayat": "فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ فَإِذَا هُمْ يَنْظُرُونَ"
},
{
    "id": "3808",
    "sura": "37",
    "VerseIDAr": "20",
    "ayat": "وَقَالُوا يَا وَيْلَنَا هَٰذَا يَوْمُ الدِّينِ"
},
{
    "id": "3809",
    "sura": "37",
    "VerseIDAr": "21",
    "ayat": "هَٰذَا يَوْمُ الْفَصْلِ الَّذِي كُنْتُمْ بِهِ تُكَذِّبُونَ"
},
{
    "id": "3810",
    "sura": "37",
    "VerseIDAr": "22",
    "ayat": "احْشُرُوا الَّذِينَ ظَلَمُوا وَأَزْوَاجَهُمْ وَمَا كَانُوا يَعْبُدُونَ"
},
{
    "id": "3811",
    "sura": "37",
    "VerseIDAr": "23",
    "ayat": "مِنْ دُونِ اللَّهِ فَاهْدُوهُمْ إِلَىٰ صِرَاطِ الْجَحِيمِ"
},
{
    "id": "3812",
    "sura": "37",
    "VerseIDAr": "24",
    "ayat": "وَقِفُوهُمْ ۖ إِنَّهُمْ مَسْئُولُونَ"
},
{
    "id": "3813",
    "sura": "37",
    "VerseIDAr": "25",
    "ayat": "مَا لَكُمْ لَا تَنَاصَرُونَ"
},
{
    "id": "3814",
    "sura": "37",
    "VerseIDAr": "26",
    "ayat": "بَلْ هُمُ الْيَوْمَ مُسْتَسْلِمُونَ"
},
{
    "id": "3815",
    "sura": "37",
    "VerseIDAr": "27",
    "ayat": "وَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَاءَلُونَ"
},
{
    "id": "3816",
    "sura": "37",
    "VerseIDAr": "28",
    "ayat": "قَالُوا إِنَّكُمْ كُنْتُمْ تَأْتُونَنَا عَنِ الْيَمِينِ"
},
{
    "id": "3817",
    "sura": "37",
    "VerseIDAr": "29",
    "ayat": "قَالُوا بَلْ لَمْ تَكُونُوا مُؤْمِنِينَ"
},
{
    "id": "3818",
    "sura": "37",
    "VerseIDAr": "30",
    "ayat": "وَمَا كَانَ لَنَا عَلَيْكُمْ مِنْ سُلْطَانٍ ۖ بَلْ كُنْتُمْ قَوْمًا طَاغِينَ"
},
{
    "id": "3819",
    "sura": "37",
    "VerseIDAr": "31",
    "ayat": "فَحَقَّ عَلَيْنَا قَوْلُ رَبِّنَا ۖ إِنَّا لَذَائِقُونَ"
},
{
    "id": "3820",
    "sura": "37",
    "VerseIDAr": "32",
    "ayat": "فَأَغْوَيْنَاكُمْ إِنَّا كُنَّا غَاوِينَ"
},
{
    "id": "3821",
    "sura": "37",
    "VerseIDAr": "33",
    "ayat": "فَإِنَّهُمْ يَوْمَئِذٍ فِي الْعَذَابِ مُشْتَرِكُونَ"
},
{
    "id": "3822",
    "sura": "37",
    "VerseIDAr": "34",
    "ayat": "إِنَّا كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَ"
},
{
    "id": "3823",
    "sura": "37",
    "VerseIDAr": "35",
    "ayat": "إِنَّهُمْ كَانُوا إِذَا قِيلَ لَهُمْ لَا إِلَٰهَ إِلَّا اللَّهُ يَسْتَكْبِرُونَ"
},
{
    "id": "3824",
    "sura": "37",
    "VerseIDAr": "36",
    "ayat": "وَيَقُولُونَ أَئِنَّا لَتَارِكُو آلِهَتِنَا لِشَاعِرٍ مَجْنُونٍ"
},
{
    "id": "3825",
    "sura": "37",
    "VerseIDAr": "37",
    "ayat": "بَلْ جَاءَ بِالْحَقِّ وَصَدَّقَ الْمُرْسَلِينَ"
},
{
    "id": "3826",
    "sura": "37",
    "VerseIDAr": "38",
    "ayat": "إِنَّكُمْ لَذَائِقُو الْعَذَابِ الْأَلِيمِ"
},
{
    "id": "3827",
    "sura": "37",
    "VerseIDAr": "39",
    "ayat": "وَمَا تُجْزَوْنَ إِلَّا مَا كُنْتُمْ تَعْمَلُونَ"
},
{
    "id": "3828",
    "sura": "37",
    "VerseIDAr": "40",
    "ayat": "إِلَّا عِبَادَ اللَّهِ الْمُخْلَصِينَ"
},
{
    "id": "3829",
    "sura": "37",
    "VerseIDAr": "41",
    "ayat": "أُولَٰئِكَ لَهُمْ رِزْقٌ مَعْلُومٌ"
},
{
    "id": "3830",
    "sura": "37",
    "VerseIDAr": "42",
    "ayat": "فَوَاكِهُ ۖ وَهُمْ مُكْرَمُونَ"
},
{
    "id": "3831",
    "sura": "37",
    "VerseIDAr": "43",
    "ayat": "فِي جَنَّاتِ النَّعِيمِ"
},
{
    "id": "3832",
    "sura": "37",
    "VerseIDAr": "44",
    "ayat": "عَلَىٰ سُرُرٍ مُتَقَابِلِينَ"
},
{
    "id": "3833",
    "sura": "37",
    "VerseIDAr": "45",
    "ayat": "يُطَافُ عَلَيْهِمْ بِكَأْسٍ مِنْ مَعِينٍ"
},
{
    "id": "3834",
    "sura": "37",
    "VerseIDAr": "46",
    "ayat": "بَيْضَاءَ لَذَّةٍ لِلشَّارِبِينَ"
},
{
    "id": "3835",
    "sura": "37",
    "VerseIDAr": "47",
    "ayat": "لَا فِيهَا غَوْلٌ وَلَا هُمْ عَنْهَا يُنْزَفُونَ"
},
{
    "id": "3836",
    "sura": "37",
    "VerseIDAr": "48",
    "ayat": "وَعِنْدَهُمْ قَاصِرَاتُ الطَّرْفِ عِينٌ"
},
{
    "id": "3837",
    "sura": "37",
    "VerseIDAr": "49",
    "ayat": "كَأَنَّهُنَّ بَيْضٌ مَكْنُونٌ"
},
{
    "id": "3838",
    "sura": "37",
    "VerseIDAr": "50",
    "ayat": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَاءَلُونَ"
},
{
    "id": "3839",
    "sura": "37",
    "VerseIDAr": "51",
    "ayat": "قَالَ قَائِلٌ مِنْهُمْ إِنِّي كَانَ لِي قَرِينٌ"
},
{
    "id": "3840",
    "sura": "37",
    "VerseIDAr": "52",
    "ayat": "يَقُولُ أَإِنَّكَ لَمِنَ الْمُصَدِّقِينَ"
},
{
    "id": "3841",
    "sura": "37",
    "VerseIDAr": "53",
    "ayat": "أَإِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَامًا أَإِنَّا لَمَدِينُونَ"
},
{
    "id": "3842",
    "sura": "37",
    "VerseIDAr": "54",
    "ayat": "قَالَ هَلْ أَنْتُمْ مُطَّلِعُونَ"
},
{
    "id": "3843",
    "sura": "37",
    "VerseIDAr": "55",
    "ayat": "فَاطَّلَعَ فَرَآهُ فِي سَوَاءِ الْجَحِيمِ"
},
{
    "id": "3844",
    "sura": "37",
    "VerseIDAr": "56",
    "ayat": "قَالَ تَاللَّهِ إِنْ كِدْتَ لَتُرْدِينِ"
},
{
    "id": "3845",
    "sura": "37",
    "VerseIDAr": "57",
    "ayat": "وَلَوْلَا نِعْمَةُ رَبِّي لَكُنْتُ مِنَ الْمُحْضَرِينَ"
},
{
    "id": "3846",
    "sura": "37",
    "VerseIDAr": "58",
    "ayat": "أَفَمَا نَحْنُ بِمَيِّتِينَ"
},
{
    "id": "3847",
    "sura": "37",
    "VerseIDAr": "59",
    "ayat": "إِلَّا مَوْتَتَنَا الْأُولَىٰ وَمَا نَحْنُ بِمُعَذَّبِينَ"
},
{
    "id": "3848",
    "sura": "37",
    "VerseIDAr": "60",
    "ayat": "إِنَّ هَٰذَا لَهُوَ الْفَوْزُ الْعَظِيمُ"
},
{
    "id": "3849",
    "sura": "37",
    "VerseIDAr": "61",
    "ayat": "لِمِثْلِ هَٰذَا فَلْيَعْمَلِ الْعَامِلُونَ"
},
{
    "id": "3850",
    "sura": "37",
    "VerseIDAr": "62",
    "ayat": "أَذَٰلِكَ خَيْرٌ نُزُلًا أَمْ شَجَرَةُ الزَّقُّومِ"
},
{
    "id": "3851",
    "sura": "37",
    "VerseIDAr": "63",
    "ayat": "إِنَّا جَعَلْنَاهَا فِتْنَةً لِلظَّالِمِينَ"
},
{
    "id": "3852",
    "sura": "37",
    "VerseIDAr": "64",
    "ayat": "إِنَّهَا شَجَرَةٌ تَخْرُجُ فِي أَصْلِ الْجَحِيمِ"
},
{
    "id": "3853",
    "sura": "37",
    "VerseIDAr": "65",
    "ayat": "طَلْعُهَا كَأَنَّهُ رُءُوسُ الشَّيَاطِينِ"
},
{
    "id": "3854",
    "sura": "37",
    "VerseIDAr": "66",
    "ayat": "فَإِنَّهُمْ لَآكِلُونَ مِنْهَا فَمَالِئُونَ مِنْهَا الْبُطُونَ"
},
{
    "id": "3855",
    "sura": "37",
    "VerseIDAr": "67",
    "ayat": "ثُمَّ إِنَّ لَهُمْ عَلَيْهَا لَشَوْبًا مِنْ حَمِيمٍ"
},
{
    "id": "3856",
    "sura": "37",
    "VerseIDAr": "68",
    "ayat": "ثُمَّ إِنَّ مَرْجِعَهُمْ لَإِلَى الْجَحِيمِ"
},
{
    "id": "3857",
    "sura": "37",
    "VerseIDAr": "69",
    "ayat": "إِنَّهُمْ أَلْفَوْا آبَاءَهُمْ ضَالِّينَ"
},
{
    "id": "3858",
    "sura": "37",
    "VerseIDAr": "70",
    "ayat": "فَهُمْ عَلَىٰ آثَارِهِمْ يُهْرَعُونَ"
},
{
    "id": "3859",
    "sura": "37",
    "VerseIDAr": "71",
    "ayat": "وَلَقَدْ ضَلَّ قَبْلَهُمْ أَكْثَرُ الْأَوَّلِينَ"
},
{
    "id": "3860",
    "sura": "37",
    "VerseIDAr": "72",
    "ayat": "وَلَقَدْ أَرْسَلْنَا فِيهِمْ مُنْذِرِينَ"
},
{
    "id": "3861",
    "sura": "37",
    "VerseIDAr": "73",
    "ayat": "فَانْظُرْ كَيْفَ كَانَ عَاقِبَةُ الْمُنْذَرِينَ"
},
{
    "id": "3862",
    "sura": "37",
    "VerseIDAr": "74",
    "ayat": "إِلَّا عِبَادَ اللَّهِ الْمُخْلَصِينَ"
},
{
    "id": "3863",
    "sura": "37",
    "VerseIDAr": "75",
    "ayat": "وَلَقَدْ نَادَانَا نُوحٌ فَلَنِعْمَ الْمُجِيبُونَ"
},
{
    "id": "3864",
    "sura": "37",
    "VerseIDAr": "76",
    "ayat": "وَنَجَّيْنَاهُ وَأَهْلَهُ مِنَ الْكَرْبِ الْعَظِيمِ"
},
{
    "id": "3865",
    "sura": "37",
    "VerseIDAr": "77",
    "ayat": "وَجَعَلْنَا ذُرِّيَّتَهُ هُمُ الْبَاقِينَ"
},
{
    "id": "3866",
    "sura": "37",
    "VerseIDAr": "78",
    "ayat": "وَتَرَكْنَا عَلَيْهِ فِي الْآخِرِينَ"
},
{
    "id": "3867",
    "sura": "37",
    "VerseIDAr": "79",
    "ayat": "سَلَامٌ عَلَىٰ نُوحٍ فِي الْعَالَمِينَ"
},
{
    "id": "3868",
    "sura": "37",
    "VerseIDAr": "80",
    "ayat": "إِنَّا كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ"
},
{
    "id": "3869",
    "sura": "37",
    "VerseIDAr": "81",
    "ayat": "إِنَّهُ مِنْ عِبَادِنَا الْمُؤْمِنِينَ"
},
{
    "id": "3870",
    "sura": "37",
    "VerseIDAr": "82",
    "ayat": "ثُمَّ أَغْرَقْنَا الْآخَرِينَ"
},
{
    "id": "3871",
    "sura": "37",
    "VerseIDAr": "83",
    "ayat": "وَإِنَّ مِنْ شِيعَتِهِ لَإِبْرَاهِيمَ"
},
{
    "id": "3872",
    "sura": "37",
    "VerseIDAr": "84",
    "ayat": "إِذْ جَاءَ رَبَّهُ بِقَلْبٍ سَلِيمٍ"
},
{
    "id": "3873",
    "sura": "37",
    "VerseIDAr": "85",
