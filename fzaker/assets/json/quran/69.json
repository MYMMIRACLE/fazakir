[
{
    "id": "5324",
    "sura": "69",
    "VerseIDAr": "1",
    "ayat": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ الْحَاقَّةُ"
},
{
    "id": "5325",
    "sura": "69",
    "VerseIDAr": "2",
    "ayat": "مَا الْحَاقَّةُ"
},
{
    "id": "5326",
    "sura": "69",
    "VerseIDAr": "3",
    "ayat": "وَمَا أَدْرَاكَ مَا الْحَاقَّةُ"
},
{
    "id": "5327",
    "sura": "69",
    "VerseIDAr": "4",
    "ayat": "كَذَّبَتْ ثَمُودُ وَعَادٌ بِالْقَارِعَةِ"
},
{
    "id": "5328",
    "sura": "69",
    "VerseIDAr": "5",
    "ayat": "فَأَمَّا ثَمُودُ فَأُهْلِكُوا بِالطَّاغِيَةِ"
},
{
    "id": "5329",
    "sura": "69",
    "VerseIDAr": "6",
    "ayat": "وَأَمَّا عَادٌ فَأُهْلِكُوا بِرِيحٍ صَرْصَرٍ عَاتِيَةٍ"
},
{
    "id": "5330",
    "sura": "69",
    "VerseIDAr": "7",
    "ayat": "سَخَّرَهَا عَلَيْهِمْ سَبْعَ لَيَالٍ وَثَمَانِيَةَ أَيَّامٍ حُسُومًا فَتَرَى الْقَوْمَ فِيهَا صَرْعَىٰ كَأَنَّهُمْ أَعْجَازُ نَخْلٍ خَاوِيَةٍ"
},
{
    "id": "5331",
    "sura": "69",
    "VerseIDAr": "8",
    "ayat": "فَهَلْ تَرَىٰ لَهُمْ مِنْ بَاقِيَةٍ"
},
{
    "id": "5332",
    "sura": "69",
    "VerseIDAr": "9",
    "ayat": "وَجَاءَ فِرْعَوْنُ وَمَنْ قَبْلَهُ وَالْمُؤْتَفِكَاتُ بِالْخَاطِئَةِ"
},
{
    "id": "5333",
    "sura": "69",
    "VerseIDAr": "10",
    "ayat": "فَعَصَوْا رَسُولَ رَبِّهِمْ فَأَخَذَهُمْ أَخْذَةً رَابِيَةً"
},
{
    "id": "5334",
    "sura": "69",
    "VerseIDAr": "11",
    "ayat": "إِنَّا لَمَّا طَغَى الْمَاءُ حَمَلْنَاكُمْ فِي الْجَارِيَةِ"
},
{
    "id": "5335",
    "sura": "69",
    "VerseIDAr": "12",
    "ayat": "لِنَجْعَلَهَا لَكُمْ تَذْكِرَةً وَتَعِيَهَا أُذُنٌ وَاعِيَةٌ"
},
{
    "id": "5336",
    "sura": "69",
    "VerseIDAr": "13",
    "ayat": "فَإِذَا نُفِخَ فِي الصُّورِ نَفْخَةٌ وَاحِدَةٌ"
},
{
    "id": "5337",
    "sura": "69",
    "VerseIDAr": "14",
    "ayat": "وَحُمِلَتِ الْأَرْضُ وَالْجِبَالُ فَدُكَّتَا دَكَّةً وَاحِدَةً"
},
{
    "id": "5338",
    "sura": "69",
    "VerseIDAr": "15",
    "ayat": "فَيَوْمَئِذٍ وَقَعَتِ الْوَاقِعَةُ"
},
{
    "id": "5339",
    "sura": "69",
    "VerseIDAr": "16",
    "ayat": "وَانْشَقَّتِ السَّمَاءُ فَهِيَ يَوْمَئِذٍ وَاهِيَةٌ"
},
{
    "id": "5340",
    "sura": "69",
    "VerseIDAr": "17",
    "ayat": "وَالْمَلَكُ عَلَىٰ أَرْجَائِهَا ۚ وَيَحْمِلُ عَرْشَ رَبِّكَ فَوْقَهُمْ يَوْمَئِذٍ ثَمَانِيَةٌ"
},
{
    "id": "5341",
    "sura": "69",
    "VerseIDAr": "18",
    "ayat": "يَوْمَئِذٍ تُعْرَضُونَ لَا تَخْفَىٰ مِنْكُمْ خَافِيَةٌ"
},
{
    "id": "5342",
    "sura": "69",
    "VerseIDAr": "19",
    "ayat": "فَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِيَمِينِهِ فَيَقُولُ هَاؤُمُ اقْرَءُوا كِتَابِيَهْ"
},
{
    "id": "5343",
    "sura": "69",
    "VerseIDAr": "20",
    "ayat": "إِنِّي ظَنَنْتُ أَنِّي مُلَاقٍ حِسَابِيَهْ"
},
{
    "id": "5344",
    "sura": "69",
    "VerseIDAr": "21",
    "ayat": "فَهُوَ فِي عِيشَةٍ رَاضِيَةٍ"
},
{
    "id": "5345",
    "sura": "69",
    "VerseIDAr": "22",
    "ayat": "فِي جَنَّةٍ عَالِيَةٍ"
},
{
    "id": "5346",
    "sura": "69",
    "VerseIDAr": "23",
    "ayat": "قُطُوفُهَا دَانِيَةٌ"
},
{
    "id": "5347",
    "sura": "69",
    "VerseIDAr": "24",
    "ayat": "كُلُوا وَاشْرَبُوا هَنِيئًا بِمَا أَسْلَفْتُمْ فِي الْأَيَّامِ الْخَالِيَةِ"
},
{
    "id": "5348",
    "sura": "69",
    "VerseIDAr": "25",
    "ayat": "وَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِشِمَالِهِ فَيَقُولُ يَا لَيْتَنِي لَمْ أُوتَ كِتَابِيَهْ"
},
{
    "id": "5349",
    "sura": "69",
    "VerseIDAr": "26",
    "ayat": "وَلَمْ أَدْرِ مَا حِسَابِيَهْ"
},
{
    "id": "5350",
    "sura": "69",
    "VerseIDAr": "27",
    "ayat": "يَا لَيْتَهَا كَانَتِ الْقَاضِيَةَ"
},
{
    "id": "5351",
    "sura": "69",
    "VerseIDAr": "28",
    "ayat": "مَا أَغْنَىٰ عَنِّي مَالِيَهْ ۜ"
},
{
    "id": "5352",
    "sura": "69",
    "VerseIDAr": "29",
    "ayat": "هَلَكَ عَنِّي سُلْطَانِيَهْ"
},
{
    "id": "5353",
    "sura": "69",
    "VerseIDAr": "30",
    "ayat": "خُذُوهُ فَغُلُّوهُ"
},
{
    "id": "5354",
    "sura": "69",
    "VerseIDAr": "31",
    "ayat": "ثُمَّ الْجَحِيمَ صَلُّوهُ"
},
{
    "id": "5355",
    "sura": "69",
    "VerseIDAr": "32",
    "ayat": "ثُمَّ فِي سِلْسِلَةٍ ذَرْعُهَا سَبْعُونَ ذِرَاعًا فَاسْلُكُوهُ"
},
{
    "id": "5356",
    "sura": "69",
    "VerseIDAr": "33",
    "ayat": "إِنَّهُ كَانَ لَا يُؤْمِنُ بِاللَّهِ الْعَظِيمِ"
},
{
    "id": "5357",
    "sura": "69",
    "VerseIDAr": "34",
    "ayat": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ الْمِسْكِينِ"
},
{
    "id": "5358",
    "sura": "69",
    "VerseIDAr": "35",
    "ayat": "فَلَيْسَ لَهُ الْيَوْمَ هَاهُنَا حَمِيمٌ"
},
{
    "id": "5359",
    "sura": "69",
    "VerseIDAr": "36",
    "ayat": "وَلَا طَعَامٌ إِلَّا مِنْ غِسْلِينٍ"
},
{
    "id": "5360",
    "sura": "69",
    "VerseIDAr": "37",
    "ayat": "لَا يَأْكُلُهُ إِلَّا الْخَاطِئُونَ"
},
{
    "id": "5361",
    "sura": "69",
    "VerseIDAr": "38",
    "ayat": "فَلَا أُقْسِمُ بِمَا تُبْصِرُونَ"
},
{
    "id": "5362",
    "sura": "69",
    "VerseIDAr": "39",
    "ayat": "وَمَا لَا تُبْصِرُونَ"
},
{
    "id": "5363",
    "sura": "69",
    "VerseIDAr": "40",
    "ayat": "إِنَّهُ لَقَوْلُ رَسُولٍ كَرِيمٍ"
},
{
    "id": "5364",
    "sura": "69",
    "VerseIDAr": "41",
    "ayat": "وَمَا هُوَ بِقَوْلِ شَاعِرٍ ۚ قَلِيلًا مَا تُؤْمِنُونَ"
},
{
    "id": "5365",
    "sura": "69",
    "VerseIDAr": "42",
    "ayat": "وَلَا بِقَوْلِ كَاهِنٍ ۚ قَلِيلًا مَا تَذَكَّرُونَ"
},
{
    "id": "5366",
    "sura": "69",
    "VerseIDAr": "43",
    "ayat": "تَنْزِيلٌ مِنْ رَبِّ الْعَالَمِينَ"
},
{
    "id": "5367",
    "sura": "69",
    "VerseIDAr": "44",
    "ayat": "وَلَوْ تَقَوَّلَ عَلَيْنَا بَعْضَ الْأَقَاوِيلِ"
},
{
    "id": "5368",
    "sura": "69",
    "VerseIDAr": "45",
    "ayat": "لَأَخَذْنَا مِنْهُ بِالْيَمِينِ"
},
{
    "id": "5369",
    "sura": "69",
    "VerseIDAr": "46",
    "ayat": "ثُمَّ لَقَطَعْنَا مِنْهُ الْوَتِينَ"
},
{
    "id": "5370",
    "sura": "69",
    "VerseIDAr": "47",
    "ayat": "فَمَا مِنْكُمْ مِنْ أَحَدٍ عَنْهُ حَاجِزِينَ"
},
{
    "id": "5371",
    "sura": "69",
    "VerseIDAr": "48",
    "ayat": "وَإِنَّهُ لَتَذْكِرَةٌ لِلْمُتَّقِينَ"
},
{
    "id": "5372",
    "sura": "69",
    "VerseIDAr": "49",
    "ayat": "وَإِنَّا لَنَعْلَمُ أَنَّ مِنْكُمْ مُكَذِّبِينَ"
},
{
    "id": "5373",
    "sura": "69",
    "VerseIDAr": "50",
    "ayat": "وَإِنَّهُ لَحَسْرَةٌ عَلَى الْكَافِرِينَ"
},
{
    "id": "5374",
    "sura": "69",
    "VerseIDAr": "51",
    "ayat": "وَإِنَّهُ لَحَقُّ الْيَقِينِ"
},
{
    "id": "5375",
    "sura": "69",
    "VerseIDAr": "52",
    "ayat": "فَسَبِّحْ بِاسْمِ رَبِّكَ الْعَظِيمِ"
}
]