[
{
    "id": "5968",
    "sura": "88",
    "VerseIDAr": "1",
    "ayat": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ هَلْ أَتَاكَ حَدِيثُ الْغَاشِيَةِ"
},
{
    "id": "5969",
    "sura": "88",
    "VerseIDAr": "2",
    "ayat": "وُجُوهٌ يَوْمَئِذٍ خَاشِعَةٌ"
},
{
    "id": "5970",
    "sura": "88",
    "VerseIDAr": "3",
    "ayat": "عَامِلَةٌ نَاصِبَةٌ"
},
{
    "id": "5971",
    "sura": "88",
    "VerseIDAr": "4",
    "ayat": "تَصْلَىٰ نَارًا حَامِيَةً"
},
{
    "id": "5972",
    "sura": "88",
    "VerseIDAr": "5",
    "ayat": "تُسْقَىٰ مِنْ عَيْنٍ آنِيَةٍ"
},
{
    "id": "5973",
    "sura": "88",
    "VerseIDAr": "6",
    "ayat": "لَيْسَ لَهُمْ طَعَامٌ إِلَّا مِنْ ضَرِيعٍ"
},
{
    "id": "5974",
    "sura": "88",
    "VerseIDAr": "7",
    "ayat": "لَا يُسْمِنُ وَلَا يُغْنِي مِنْ جُوعٍ"
},
{
    "id": "5975",
    "sura": "88",
    "VerseIDAr": "8",
    "ayat": "وُجُوهٌ يَوْمَئِذٍ نَاعِمَةٌ"
},
{
    "id": "5976",
    "sura": "88",
    "VerseIDAr": "9",
    "ayat": "لِسَعْيِهَا رَاضِيَةٌ"
},
{
    "id": "5977",
    "sura": "88",
    "VerseIDAr": "10",
    "ayat": "فِي جَنَّةٍ عَالِيَةٍ"
},
{
    "id": "5978",
    "sura": "88",
    "VerseIDAr": "11",
    "ayat": "لَا تَسْمَعُ فِيهَا لَاغِيَةً"
},
{
    "id": "5979",
    "sura": "88",
    "VerseIDAr": "12",
    "ayat": "فِيهَا عَيْنٌ جَارِيَةٌ"
},
{
    "id": "5980",
    "sura": "88",
    "VerseIDAr": "13",
    "ayat": "فِيهَا سُرُرٌ مَرْفُوعَةٌ"
},
{
    "id": "5981",
    "sura": "88",
    "VerseIDAr": "14",
    "ayat": "وَأَكْوَابٌ مَوْضُوعَةٌ"
},
{
    "id": "5982",
    "sura": "88",
    "VerseIDAr": "15",
    "ayat": "وَنَمَارِقُ مَصْفُوفَةٌ"
},
{
    "id": "5983",
    "sura": "88",
    "VerseIDAr": "16",
    "ayat": "وَزَرَابِيُّ مَبْثُوثَةٌ"
},
{
    "id": "5984",
    "sura": "88",
    "VerseIDAr": "17",
    "ayat": "أَفَلَا يَنْظُرُونَ إِلَى الْإِبِلِ كَيْفَ خُلِقَتْ"
},
{
    "id": "5985",
    "sura": "88",
    "VerseIDAr": "18",
    "ayat": "وَإِلَى السَّمَاءِ كَيْفَ رُفِعَتْ"
},
{
    "id": "5986",
    "sura": "88",
    "VerseIDAr": "19",
    "ayat": "وَإِلَى الْجِبَالِ كَيْفَ نُصِبَتْ"
},
{
    "id": "5987",
    "sura": "88",
    "VerseIDAr": "20",
    "ayat": "وَإِلَى الْأَرْضِ كَيْفَ سُطِحَتْ"
},
{
    "id": "5988",
    "sura": "88",
    "VerseIDAr": "21",
    "ayat": "فَذَكِّرْ إِنَّمَا أَنْتَ مُذَكِّرٌ"
},
{
    "id": "5989",
    "sura": "88",
    "VerseIDAr": "22",
    "ayat": "لَسْتَ عَلَيْهِمْ بِمُصَيْطِرٍ"
},
{
    "id": "5990",
    "sura": "88",
    "VerseIDAr": "23",
    "ayat": "إِلَّا مَنْ تَوَلَّىٰ وَكَفَرَ"
},
{
    "id": "5991",
    "sura": "88",
    "VerseIDAr": "24",
    "ayat": "فَيُعَذِّبُهُ اللَّهُ الْعَذَابَ الْأَكْبَرَ"
},
{
    "id": "5992",
    "sura": "88",
    "VerseIDAr": "25",
    "ayat": "إِنَّ إِلَيْنَا إِيَابَهُمْ"
},
{
    "id": "5993",
    "sura": "88",
    "VerseIDAr": "26",
    "ayat": "ثُمَّ إِنَّ عَلَيْنَا حِسَابَهُمْ"
}
]