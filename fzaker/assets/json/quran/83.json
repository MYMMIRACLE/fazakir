[
{
    "id": "5849",
    "sura": "83",
    "VerseIDAr": "1",
    "ayat": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَيْلٌ لِلْمُطَفِّفِينَ"
},
{
    "id": "5850",
    "sura": "83",
    "VerseIDAr": "2",
    "ayat": "الَّذِينَ إِذَا اكْتَالُوا عَلَى النَّاسِ يَسْتَوْفُونَ"
},
{
    "id": "5851",
    "sura": "83",
    "VerseIDAr": "3",
    "ayat": "وَإِذَا كَالُوهُمْ أَوْ وَزَنُوهُمْ يُخْسِرُونَ"
},
{
    "id": "5852",
    "sura": "83",
    "VerseIDAr": "4",
    "ayat": "أَلَا يَظُنُّ أُولَٰئِكَ أَنَّهُمْ مَبْعُوثُونَ"
},
{
    "id": "5853",
    "sura": "83",
    "VerseIDAr": "5",
    "ayat": "لِيَوْمٍ عَظِيمٍ"
},
{
    "id": "5854",
    "sura": "83",
    "VerseIDAr": "6",
    "ayat": "يَوْمَ يَقُومُ النَّاسُ لِرَبِّ الْعَالَمِينَ"
},
{
    "id": "5855",
    "sura": "83",
    "VerseIDAr": "7",
    "ayat": "كَلَّا إِنَّ كِتَابَ الْفُجَّارِ لَفِي سِجِّينٍ"
},
{
    "id": "5856",
    "sura": "83",
    "VerseIDAr": "8",
    "ayat": "وَمَا أَدْرَاكَ مَا سِجِّينٌ"
},
{
    "id": "5857",
    "sura": "83",
    "VerseIDAr": "9",
    "ayat": "كِتَابٌ مَرْقُومٌ"
},
{
    "id": "5858",
    "sura": "83",
    "VerseIDAr": "10",
    "ayat": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
},
{
    "id": "5859",
    "sura": "83",
    "VerseIDAr": "11",
    "ayat": "الَّذِينَ يُكَذِّبُونَ بِيَوْمِ الدِّينِ"
},
{
    "id": "5860",
    "sura": "83",
    "VerseIDAr": "12",
    "ayat": "وَمَا يُكَذِّبُ بِهِ إِلَّا كُلُّ مُعْتَدٍ أَثِيمٍ"
},
{
    "id": "5861",
    "sura": "83",
    "VerseIDAr": "13",
    "ayat": "إِذَا تُتْلَىٰ عَلَيْهِ آيَاتُنَا قَالَ أَسَاطِيرُ الْأَوَّلِينَ"
},
{
    "id": "5862",
    "sura": "83",
    "VerseIDAr": "14",
    "ayat": "كَلَّا ۖ بَلْ ۜ رَانَ عَلَىٰ قُلُوبِهِمْ مَا كَانُوا يَكْسِبُونَ"
},
{
    "id": "5863",
    "sura": "83",
    "VerseIDAr": "15",
    "ayat": "كَلَّا إِنَّهُمْ عَنْ رَبِّهِمْ يَوْمَئِذٍ لَمَحْجُوبُونَ"
},
{
    "id": "5864",
    "sura": "83",
    "VerseIDAr": "16",
    "ayat": "ثُمَّ إِنَّهُمْ لَصَالُو الْجَحِيمِ"
},
{
    "id": "5865",
    "sura": "83",
    "VerseIDAr": "17",
    "ayat": "ثُمَّ يُقَالُ هَٰذَا الَّذِي كُنْتُمْ بِهِ تُكَذِّبُونَ"
},
{
    "id": "5866",
    "sura": "83",
    "VerseIDAr": "18",
    "ayat": "كَلَّا إِنَّ كِتَابَ الْأَبْرَارِ لَفِي عِلِّيِّينَ"
},
{
    "id": "5867",
    "sura": "83",
    "VerseIDAr": "19",
    "ayat": "وَمَا أَدْرَاكَ مَا عِلِّيُّونَ"
},
{
    "id": "5868",
    "sura": "83",
    "VerseIDAr": "20",
    "ayat": "كِتَابٌ مَرْقُومٌ"
},
{
    "id": "5869",
    "sura": "83",
    "VerseIDAr": "21",
    "ayat": "يَشْهَدُهُ الْمُقَرَّبُونَ"
},
{
    "id": "5870",
    "sura": "83",
    "VerseIDAr": "22",
    "ayat": "إِنَّ الْأَبْرَارَ لَفِي نَعِيمٍ"
},
{
    "id": "5871",
    "sura": "83",
    "VerseIDAr": "23",
    "ayat": "عَلَى الْأَرَائِكِ يَنْظُرُونَ"
},
{
    "id": "5872",
    "sura": "83",
    "VerseIDAr": "24",
    "ayat": "تَعْرِفُ فِي وُجُوهِهِمْ نَضْرَةَ النَّعِيمِ"
},
{
    "id": "5873",
    "sura": "83",
    "VerseIDAr": "25",
    "ayat": "يُسْقَوْنَ مِنْ رَحِيقٍ مَخْتُومٍ"
},
{
    "id": "5874",
    "sura": "83",
    "VerseIDAr": "26",
    "ayat": "خِتَامُهُ مِسْكٌ ۚ وَفِي ذَٰلِكَ فَلْيَتَنَافَسِ الْمُتَنَافِسُونَ"
},
{
    "id": "5875",
    "sura": "83",
    "VerseIDAr": "27",
    "ayat": "وَمِزَاجُهُ مِنْ تَسْنِيمٍ"
},
{
    "id": "5876",
    "sura": "83",
    "VerseIDAr": "28",
    "ayat": "عَيْنًا يَشْرَبُ بِهَا الْمُقَرَّبُونَ"
},
{
    "id": "5877",
    "sura": "83",
    "VerseIDAr": "29",
    "ayat": "إِنَّ الَّذِينَ أَجْرَمُوا كَانُوا مِنَ الَّذِينَ آمَنُوا يَضْحَكُونَ"
},
{
    "id": "5878",
    "sura": "83",
    "VerseIDAr": "30",
    "ayat": "وَإِذَا مَرُّوا بِهِمْ يَتَغَامَزُونَ"
},
{
    "id": "5879",
    "sura": "83",
    "VerseIDAr": "31",
    "ayat": "وَإِذَا انْقَلَبُوا إِلَىٰ أَهْلِهِمُ انْقَلَبُوا فَكِهِينَ"
},
{
    "id": "5880",
    "sura": "83",
    "VerseIDAr": "32",
    "ayat": "وَإِذَا رَأَوْهُمْ قَالُوا إِنَّ هَٰؤُلَاءِ لَضَالُّونَ"
},
{
    "id": "5881",
    "sura": "83",
    "VerseIDAr": "33",
    "ayat": "وَمَا أُرْسِلُوا عَلَيْهِمْ حَافِظِينَ"
},
{
    "id": "5882",
    "sura": "83",
    "VerseIDAr": "34",
    "ayat": "فَالْيَوْمَ الَّذِينَ آمَنُوا مِنَ الْكُفَّارِ يَضْحَكُونَ"
},
{
    "id": "5883",
    "sura": "83",
    "VerseIDAr": "35",
    "ayat": "عَلَى الْأَرَائِكِ يَنْظُرُونَ"
},
{
    "id": "5884",
    "sura": "83",
    "VerseIDAr": "36",
    "ayat": "هَلْ ثُوِّبَ الْكُفَّارُ مَا كَانُوا يَفْعَلُونَ"
}
]