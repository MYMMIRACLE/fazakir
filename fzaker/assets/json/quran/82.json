[
{
    "id": "5830",
    "sura": "82",
    "VerseIDAr": "1",
    "ayat": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ إِذَا السَّمَاءُ انْفَطَرَتْ"
},
{
    "id": "5831",
    "sura": "82",
    "VerseIDAr": "2",
    "ayat": "وَإِذَا الْكَوَاكِبُ انْتَثَرَتْ"
},
{
    "id": "5832",
    "sura": "82",
    "VerseIDAr": "3",
    "ayat": "وَإِذَا الْبِحَارُ فُجِّرَتْ"
},
{
    "id": "5833",
    "sura": "82",
    "VerseIDAr": "4",
    "ayat": "وَإِذَا الْقُبُورُ بُعْثِرَتْ"
},
{
    "id": "5834",
    "sura": "82",
    "VerseIDAr": "5",
    "ayat": "عَلِمَتْ نَفْسٌ مَا قَدَّمَتْ وَأَخَّرَتْ"
},
{
    "id": "5835",
    "sura": "82",
    "VerseIDAr": "6",
    "ayat": "يَا أَيُّهَا الْإِنْسَانُ مَا غَرَّكَ بِرَبِّكَ الْكَرِيمِ"
},
{
    "id": "5836",
    "sura": "82",
    "VerseIDAr": "7",
    "ayat": "الَّذِي خَلَقَكَ فَسَوَّاكَ فَعَدَلَكَ"
},
{
    "id": "5837",
    "sura": "82",
    "VerseIDAr": "8",
    "ayat": "فِي أَيِّ صُورَةٍ مَا شَاءَ رَكَّبَكَ"
},
{
    "id": "5838",
    "sura": "82",
    "VerseIDAr": "9",
    "ayat": "كَلَّا بَلْ تُكَذِّبُونَ بِالدِّينِ"
},
{
    "id": "5839",
    "sura": "82",
    "VerseIDAr": "10",
    "ayat": "وَإِنَّ عَلَيْكُمْ لَحَافِظِينَ"
},
{
    "id": "5840",
    "sura": "82",
    "VerseIDAr": "11",
    "ayat": "كِرَامًا كَاتِبِينَ"
},
{
    "id": "5841",
    "sura": "82",
    "VerseIDAr": "12",
    "ayat": "يَعْلَمُونَ مَا تَفْعَلُونَ"
},
{
    "id": "5842",
    "sura": "82",
    "VerseIDAr": "13",
    "ayat": "إِنَّ الْأَبْرَارَ لَفِي نَعِيمٍ"
},
{
    "id": "5843",
    "sura": "82",
    "VerseIDAr": "14",
    "ayat": "وَإِنَّ الْفُجَّارَ لَفِي جَحِيمٍ"
},
{
    "id": "5844",
    "sura": "82",
    "VerseIDAr": "15",
    "ayat": "يَصْلَوْنَهَا يَوْمَ الدِّينِ"
},
{
    "id": "5845",
    "sura": "82",
    "VerseIDAr": "16",
    "ayat": "وَمَا هُمْ عَنْهَا بِغَائِبِينَ"
},
{
    "id": "5846",
    "sura": "82",
    "VerseIDAr": "17",
    "ayat": "وَمَا أَدْرَاكَ مَا يَوْمُ الدِّينِ"
},
{
    "id": "5847",
    "sura": "82",
    "VerseIDAr": "18",
    "ayat": "ثُمَّ مَا أَدْرَاكَ مَا يَوْمُ الدِّينِ"
},
{
    "id": "5848",
    "sura": "82",
    "VerseIDAr": "19",
    "ayat": "يَوْمَ لَا تَمْلِكُ نَفْسٌ لِنَفْسٍ شَيْئًا ۖ وَالْأَمْرُ يَوْمَئِذٍ لِلَّهِ"
}
]